  publisher = {Association for Computing Machinery},
  series = {ICAITE '24},
  title = {Research on the Analysis and Application of Artificial Intelligence and Teacher Collaborative Teaching System Model},
  url = {https://doi.org/10.1145/3702386.3702402},
  year = {2025}
}

@inproceedings{merged1337,
  abstract = {With the rapid growth of the global digital economy, the digital transformation of tertiary industries, including culture and tourism, has become increasingly prevalent. The digitalization of marketing makes room for the artificial intelligence (AI) and data driven smart marketing strategy to change the face of several industries, including intangible cultural heritage (ICH). This study aims at exploring the way in which AI driven marketing can promote Chaoshan cuisine, a world famous intangible cultural heritage, towards industrialization. The study uses AI based consumer analysis, targeted advertising and short video content planning to test the digital strategies that drive economic sustainability in traditional culinary sector. Using descriptive statistics, correlation analysis, and regression analysis on questionnaire data, the research verifies the promotion effects of brand and consumers through AI-driven, data-based short videos for marketing Chaoshan cuisine. Short video marketing is proven to increase brand awareness and user interest and the application of AI driven targeted marketing will promote the actual behavior of consumers, according to findings. The study also puts forward practical strategies and optimization suggestions of smart marketing, combining theoretical and empirical support for the sustainable industrialization of intangible cultural heritage cuisine.},
