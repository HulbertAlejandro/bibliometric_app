  title = {Prediction based on multi - model agricultural assistance recommendation algorithms},
  url = {https://doi.org/10.1145/3728725.3728777},
  year = {2025}
}

@inproceedings{merged601,
  abstract = {Offline reinforcement learning aims to empower agents to derive effective strategies from a pre-existing dataset for decision-making tasks. This learning paradigm has a broad application prospect in areas with high safety constraints, such as healthcare and robotic control. However, existing offline reinforcement learning algorithms often overlook the impact of the dataset's inherent multimodal distribution on policy optimization during the training phase, leading to compromised model performance. To tackle this challenge, we introduce a novel offline reinforcement learning algorithm based on mixture density policy network, MDPo ( Mixture Density Policy). The MDPo algorithm initially employs an expected regression loss function to train the value function. Subsequently, it constructs the policy using a mixture density network and trains the policy through a distributional constraint, ultimately learning a high-quality policy model under the combined influence of the reward signal and policy constraints. Predominantly leveraging mixture density networks, MDPo models the policy as a multimodal distribution, enhancing the policy's representational capacity to better fit the multimodal distribution of actions in the dataset, thereby increasing training process stability and improving model performance. Experiments conducted on the Antmaze task of the D4RL benchmark demonstrate that MDPo significantly outperforms existing state-of-the-art methods, also demonstrating the enhancement of training stability.},
  address = {New York, NY, USA},
  author = {Liu, Chen and Wang, Yizhuo},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3665348.3665372},
  isbn = {9798400709562},
  location = {Kuala Lumpur, Malaysia},
  numpages = {6},
  pages = {132–137},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '24},
  title = {MDPo: Offline Reinforcement Learning Based on Mixture Density Policy Network},
  url = {https://doi.org/10.1145/3665348.3665372},
  year = {2024}
}

@inproceedings{merged602,
  abstract = {In order to solve the problem of low segmentation accuracy due to the interference of noise information such as light and shadow in the current lightweight blind segmentation model, we propose a fast tactile paving segmentation model based on reparameterize. The multi-branch training structure learns spatial features such as tactile paving, light and shadow while compensating for the loss of information in the feature map due to downsampling operations as well as enhancing the learning of its features in the channel dimension. The single-branch structure obtained after reparameterization of the multibranch structure ensures faster inference of the model as well as lower memory access costs. The introduction of coordinate attention allows the model to learn the distribution law of the features of tactile paving, light and shadow, etc. in the spatial dimension and automatically generate the attention map with weight information, effectively reducing the weight of the spatial structural features of noise such as light and shadow. Experiments show that the model's ability to resist interference from environmental noise such as light and shadow has been enhanced and the segmentation accuracy has been further improved under the premise of ensuring the inference speed.},
  address = {New York, NY, USA},
  author = {Niu, Lianqiang and Bao, Han},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3665348.3665354},
  isbn = {9798400709562},
  location = {Kuala Lumpur, Malaysia},
  numpages = {5},
  pages = {24–28},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '24},
  title = {Fast Tactile Paving Segmentation Model Based on Reparameterized Structure},
  url = {https://doi.org/10.1145/3665348.3665354},
  year = {2024}
}

@inproceedings{merged603,
  abstract = {In response to the issue of multilingual interference in machine translation of minority languages in China, this study proposes an innovative solution based on the Transformer model. The strategy integrates adapter technology by incorporating a small-parameter adapter module on the main model, introducing highly adaptive external components tailored to each language pair to alleviate multilingual interference without altering the main model structure. Experimental results demonstrate the effectiveness of this approach in multilingual translation tasks involving Mongolian, Tibetan, Uyghur, and Chinese. Compared to traditional multilingual baseline models, this method achieves a significant improvement with an average BLEU score increase of up to 1.34 in many-to-one translation scenarios and 0.7 in one-to-many translation scenarios. This work not only provides strong support for addressing multilingual interference in machine translation of minority languages in China but also opens up new avenues and methods for multilingual translation research in other language pairs.},
  address = {New York, NY, USA},
  author = {Jin, Bo},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3665348.3665355},
  isbn = {9798400709562},
  location = {Kuala Lumpur, Malaysia},
  numpages = {7},
  pages = {29–35},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '24},
  title = {Multilingual Neural Machine Translation with Integrated Language Adapters},
  url = {https://doi.org/10.1145/3665348.3665355},
  year = {2024}
}

@inproceedings{merged604,
  abstract = {Music recommendation systems have become one of the research directions that have attracted much attention in the field of digital entertainment. However, current music recommendation methods are mainly recommended through collaborative filtering, content-based, and manual rule methods, and cannot be recommended based on the user's real-time status, resulting in the recommended music does not meet the real-time needs of users. In response to this situation, this paper proposes a personalized music recommendation model based on human physiological signals. This model first analyzes the user's physiological data, such as heart rate, electrodermal activity, etc., and uses a graph convolutional neural network algorithm to perceive the user's real-time emotional state. It then achieves more accuracy and personalization music recommendations based on the relationship model between emotion and music selection. This research not only expands the research scope in the field of music recommendation, but also provides new ideas and methods for the further development of intelligent music recommendation systems.},
  address = {New York, NY, USA},
  author = {Wang, Yingqiang and Serrano, Elcid},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3665348.3665365},
  isbn = {9798400709562},
  location = {Kuala Lumpur, Malaysia},
  numpages = {5},
  pages = {89–93},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '24},
  title = {Research on personalized music recommendation model based on human physiological signals},
  url = {https://doi.org/10.1145/3665348.3665365},
  year = {2024}
}

@inproceedings{merged605,
  abstract = {In response to the issues of existing face liveness detection models, such as performance degradation under complex lighting conditions and poor generalization performance across datasets, we propose a 3D Mask Detection Algorithm by introducing attention mechanism and feature fusion methods. This approach is designed based on FlowNet2 and MobileNetV3. Firstly, the improved optical flow model is used to obtain optical flow feature maps, which are then convolved to calculate frames with larger motion trajectories. Weighted allocation is performed for spatial features of multiple frames, followed by spatial feature aggregation. Secondly, the improved MobileNetV3 is employed to process spatio-temporal features and obtain deep features. A feature fusion method based on attention mechanism is used to fuse spatio-temporal features. Finally, the fused features are inputted into a classifier to obtain prediction scores. Compared with classical face liveness detection methods, our model exhibits better detection performance and generalization ability. In experiments on the HiFiMask dataset, the minimum detection error rate is only 2.07%, outperforming classical detection methods such as CDCN++ and LeTSrPPG. Our face liveness detection method captures the abnormal traces of 3D masks spliced with faces through the optical flow features obtained by the optical flow model and the fusion of spatio-temporal information. This method is insensitive to lighting conditions and performs excellently in cross-dataset experiments, demonstrating a certain level of generalization performance.},
  address = {New York, NY, USA},
  author = {Zhou, Yiyan and Yu, Xiaoyu and Shi, Liang},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3665348.3665412},
  isbn = {9798400709562},
  location = {Kuala Lumpur, Malaysia},
  numpages = {5},
  pages = {375–379},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '24},
  title = {3D Mask Detection Algorithm Based on Spatio-Temporal Information Fusion},
  url = {https://doi.org/10.1145/3665348.3665412},
  year = {2024}
}

@inproceedings{merged606,
  abstract = {Generative Artificial Intelligence (GenAI) applications in artistic and creative domains have gained substantial attention of late. These intelligent interactive systems, shaped by innovations in Large Language Models (LLMs) and Vision Language Models (VLMs), are materially impacting digital creative domains. While initial work to understand this space has highlighted new models and architectures, we lack a holistic view of how interactive GenAI systems are designed for user interactions across various artistic and creative domains. In this paper, we present a systematic review of interactive GenAI system designs for art and creativity in the HCI literature (N = 189), and a detailed taxonomy of interaction paradigms with design components. We shed light on the communities of design focus and decompose the system interaction designs, mapping these characteristics to creative domains, user interaction patterns, GenAI technologies, detailing under-represented spaces, and future directions of designing interactions for GenAI creativity.},
  address = {New York, NY, USA},
  author = {Hu, Xi and Xing, Yiwen and Cai, Xudong and Zhao, Yihang and Cook, Michael and Borgo, Rita and Neate, Timothy},
  booktitle = {Proceedings of the 2025 ACM Designing Interactive Systems Conference},
  doi = {10.1145/3715336.3735843},
  isbn = {9798400714856},
  keywords = {Generative AI; art; creativity; systematic review; taxonomy.},
  numpages = {30},
  pages = {1126–1155},
  publisher = {Association for Computing Machinery},
  series = {DIS '25},
  title = {Designing Interactions with Generative AI for Art and Creativity: A Systematic Review and Taxonomy},
  url = {https://doi.org/10.1145/3715336.3735843},
  year = {2025}
}

@inproceedings{merged607,
  abstract = {Contrastive Language-Image Pre-training (CLIP) model has been widely used in the field of dense image prediction tasks. However, adapting the upstream large-scale pre-training model to downstream tasks with specific requirements, such as building extraction, often encounters significant challenges due to the complex spatial relationships present in remote sensing images. To address this issue, we proposed a novel building extraction network named Natural Language Guided Building Extraction with Contrastive Language-Image Pre-training (NLG-ExtractClip) to use natural language to guide building extraction. Specifically, we introduce a Natural Language Guiding Module (NLGM) to generate remote sensing image captions with rich spatial information, and then use CLIP Text Encoder to generate Guiding Text Embeddings. We use a Multimodal Fusion Attention Module (MFAM) to combine textual information with the visual features of remote sensing images to adapt to building extraction specific tasks. Our experiments on two building extraction datasets show that our proposed method performs better than other compared methods.},
  address = {New York, NY, USA},
  author = {Wang, Chenhe},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3665348.3665360},
  isbn = {9798400709562},
  location = {Kuala Lumpur, Malaysia},
  numpages = {8},
  pages = {59–66},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '24},
  title = {NLG-ExtractClip: Natural Language Guided Building Extraction with Contrastive Language-Image Pre-training},
  url = {https://doi.org/10.1145/3665348.3665360},
  year = {2024}
}

@inproceedings{merged608,
  abstract = {In recent years, Online Detection of Action Start has attracted considerable interest owing to its diverse applications in areas like healthcare and smart transportation. However, the inherent limitation of detection tasks—"detection can only occur after an event has happened"—prevents us from obtaining responses and issuing warnings before certain events, particularly emergencies, occur. In order to mitigate this constraint, this paper introduces the Prediction of Action Start (PAS) task based on ODAS. The PAS task aims to predict the start times and categories of future actions (if any) in untrimmed streaming videos containing substantial background footage, based on recently observed video information. Our&nbsp;experimental results on two standard datasets demonstrate that the proposed prediction method even outperforms some current action detection methods.},
  address = {New York, NY, USA},
  author = {Zhang, Shizhan and Liu, Sheng and Wang, Shijie and Du, Sidan},
