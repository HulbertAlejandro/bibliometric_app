  numpages = {5},
  pages = {158–162},
  publisher = {Association for Computing Machinery},
  series = {SIGDOC '25},
  title = {Artificial Intelligence and Rhetoric: Investigating Contextual Influence on Technological Design},
  url = {https://doi.org/10.1145/3711670.3764633},
  year = {2025}
}

@inproceedings{merged1177,
  abstract = {In this paper has been investigated the relationship of the global index of artificial intelligence and the level of employment: a cluster approach in assessing cross-country differences. In the calculations of the index under con-sideration, four countries of the post-Soviet period were presented, which were divided according to the results: three of them (Latvia, Russia and Estonia) were included in the second cluster, which has an intermediate level of the Global AI Index, and one country (Armenia) was included in the third cluster, value index for which it turned out to be minimal. For the most representative (second) cluster of countries, it was found that the strength of the connection between the assessment of artificial intelligence and the level of employment is weak; the ratio of these indicators is very different across countries.},
