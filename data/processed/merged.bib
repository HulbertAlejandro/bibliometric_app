  publisher = {Association for Computing Machinery},
  series = {dg.o '24},
  title = {Comparative Analysis of Generative AI Risks in the Public Sector},
  url = {https://doi.org/10.1145/3657054.3657125},
  year = {2024}
}

@inproceedings{merged1176,
  abstract = {This paper builds upon Lynette Hunter's foundational argument that artificial intelligence (AI) lacks rhetorical “stance” by emphasizing the essential role of context in both human reasoning and the development of technological systems. Drawing on rhetorical theory and cognitive science, particularly the interactionist model of reasoning by Dan Sperber and Hugo Mercier, we challenge reductive, context-free understandings of rationality embedded in AI and platform-driven algorithmic ecologies. We argue that contemporary digital technologies, especially algorithmic systems underpinning platforms, operate through a rhetoric that flattens nuance, shapes user behavior, and undermines democratic participation. By invoking Philip Wander's concept of “rhetorical contextualization,” the paper highlights how rhetorical inquiry can uncover what is omitted, censored, or rendered invisible in technological design. We advocate for a reimagined Semantic Web, one guided by critical, ethical, and context-aware frameworks, to realize AI's potential for human flourishing. Ultimately, we position rhetorical theory not only as a tool of critique, but as a blueprint for designing more just and responsive digital futures.},
  address = {New York, NY, USA},
  author = {Tetu, Imari Cheyne and Ristich, Mike and McArdle, Casey and Potts, Liza},
  booktitle = {Proceedings of the 43rd ACM International Conference on Design of Communication},
  doi = {10.1145/3711670.3764633},
