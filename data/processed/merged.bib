  series = {GADM '25},
  title = {AI-Driven VR Design: Design of an Educational Game for Aesthetic Ambience in Ancient Chinese Poetry},
  url = {https://doi.org/10.1145/3734921.3734922},
  year = {2025}
}

@inproceedings{merged567,
  abstract = {This work presents an approach to integrating generative artificial intelligence (GenAI) into computer and data science courses. We guide students to use GenAI tools to learn key concepts and to complete low-stakes homework assignments. To assess whether students are partnering with GenAI rather than merely automating crucial learning processes, students take rapid-fire weekly quizzes where they demonstrate their mastery of the content. We conclude by reflecting on the strengths and limitations of our approach.},
