  author = {Qi, Nairu and Song, Pengfei and Mei, Tao},
  booktitle = {Proceedings of the 2024 8th International Conference on Big Data and Internet of Things},
  doi = {10.1145/3697355.3697409},
  isbn = {9798400717529},
  keywords = {artificial intelligence; human resource management; overseas engineering},
  numpages = {6},
  pages = {332–337},
  publisher = {Association for Computing Machinery},
  series = {BDIOT '24},
  title = {Practice and Innovation of Digital Application of Overseas Human Resource Management Based on Artificial Intelligence},
  url = {https://doi.org/10.1145/3697355.3697409},
  year = {2024}
}

