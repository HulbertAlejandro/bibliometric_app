  author = {Li, Min and Zhang, Heng},
  booktitle = {Proceedings of the 2024 9th International Conference on Distance Education and Learning},
  doi = {10.1145/3675812.3675870},
  isbn = {9798400716805},
  keywords = {Generative artificial intelligence; UTAUT model; behavioral intention; primary and secondary school teachers},
  location = {Guangzhou, China},
