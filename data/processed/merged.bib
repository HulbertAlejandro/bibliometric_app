  numpages = {10},
  pages = {299–308},
  publisher = {Association for Computing Machinery},
  series = {ICETM '24},
  title = {A Design Scheme of STEM Digital Textbook Based on Metaverse and Generative Artificial Intelligence},
  url = {https://doi.org/10.1145/3711403.3711453},
