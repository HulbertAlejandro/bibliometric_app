  series = {ICEKIM '25},
  title = {Design and Implementation of CDIO Teaching Empowered by Generative Artificial Intelligence: Taking the Course of “Post-Production Visual Effects” as an Example},
  url = {https://doi.org/10.1145/3756580.3756652},
  year = {2025}
}

