  booktitle = {Proceedings of the 2024 4th International Conference on Human-Machine Interaction},
  doi = {10.1145/3678429.3678436},
  isbn = {9798400716812},
  keywords = {AIGC; Artificial intelligence; Human-machine interaction; Image-text multimodal; Machine translation},
