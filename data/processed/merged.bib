  author = {Li, Pengfei and Cao, Fucheng and Yu, Shuaibei and Wang, Xinzui and Zhong, Xiaoying and Wu, Weijun},
  booktitle = {Proceedings of the 2025 International Conference on Digital Management and Information Technology},
  doi = {10.1145/3736426.3736491},
  isbn = {9798400714238},
  keywords = {Accessibility Services; Artificial Intelligence (AI); Individuals with Disabilities; Science Museum},
