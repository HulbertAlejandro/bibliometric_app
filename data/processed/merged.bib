  publisher = {Association for Computing Machinery},
  series = {DEAI '25},
  title = {Research on the Mechanism and Path of Empowering Rural Industrial Chain and Supply Chain Resilience with Generative Artificial Intelligence},
  url = {https://doi.org/10.1145/3745238.3745312},
  year = {2025}
}

