}

@inproceedings{merged74,
  abstract = {Computational thinking (CT) has emerged as a pivotal competency for navigating complex problem-solving in the digital era, yet its cultivation demands pedagogical innovations that bridge abstract concepts with tangible practice. Design-Based Learning (DBL) and Virtual Reality (VR) hold transformative potential by embedding iterative design cycles within immersive environments, prompting this study to develop a Design-Based Virtual Reality Learning (DBVRL) model that synergizes embodied VR interactions with DBL-driven tasks to enhance CT through a drone programming curriculum.},
