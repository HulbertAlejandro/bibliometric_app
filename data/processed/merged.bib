}

@inproceedings{merged1055,
  abstract = {Has AI surpassed humans in creative idea generation? This question has gained traction as generative AI (GenAI) has become widely used to support creativity. To evaluate this, we examined the first wave of experimental studies comparing human and GenAI-prompted creative idea generation by conducting a meta-analysis of 17 studies comprising 115 effect sizes. The results showed a small but non-significant pooled effect favoring GenAI. Initial analyses suggested greater originality in GenAI ideas, but sensitivity analysis showed this was driven by a few studies with very large effects. No significant differences were found between human ideas and those generated by prompting specific GenAI models (GPT-3, GPT-3.5, GPT-4). Funnel plots and asymmetry tests indicated no evidence of publication bias, supporting the findings’ validity. This meta-analysis finds no empirical support suggesting that GenAI has surpassed humans in creative idea generation. We discuss sociotechnical and sociocultural approaches as crucial for shaping human and artificial creativity.},
  address = {New York, NY, USA},
  articleno = {10},
  author = {de Rooij, Alwin and Biskjaer, Michael Mose},
  booktitle = {Proceedings of the 36th Annual Conference of the European Association of Cognitive Ergonomics},
  doi = {10.1145/3746175.3746189},
  isbn = {9798400720338},
  keywords = {Creativity; Generative Artificial Intelligence; Idea Generation; Meta-analysis},
  numpages = {11},
  publisher = {Association for Computing Machinery},
  series = {ECCE '25},
  title = {Has AI Surpassed Humans in Creative Idea Generation? A Meta-Analysis},
  url = {https://doi.org/10.1145/3746175.3746189},
  year = {2025}
}

@inproceedings{merged1056,
  abstract = {This paper discusses the adoption of Artificial Intelligence (AI) in the context of enabling accessibility services to people with disabilities in science museums, summarizing the potential benefits and the risks it entails; and suggest a fair, efficient, and strategic form of the service. The results of research show that AI technologies have significantly increased the experience for people with hearing impairments, visual impairments, mobility limitations and intellectual disabilities visiting the museum. These technologies create more equal sharing in Science Education by personalizing and letting these process interactively. Intelligent audio-guidance systems, image who recognised technology and tactile models are all specific applications were designed to make a more accessible environment for visitors with disabilities. Nevertheless, data privacy breaches and inequities during the technology deployment should be effectively addressed. Future research should explore how one may balance technological innovation with risk management with respect to making AI safe and reliable to augment accessibility of all people living with disabilities.},
