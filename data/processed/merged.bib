  numpages = {13},
  pages = {283–295},
  publisher = {Association for Computing Machinery},
  series = {FAccT '25},
  title = {Robustness and Cybersecurity in the EU Artificial Intelligence Act},
  url = {https://doi.org/10.1145/3715275.3732020},
  year = {2025}
}

