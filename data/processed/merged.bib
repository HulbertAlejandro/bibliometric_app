  keywords = {Post cinema; artificial intelligence generated films; deepfakes; expanded cinema; future cinema; generative artificial intelligence},
  numpages = {2},
  publisher = {Association for Computing Machinery},
  series = {VINCI '24},
  title = {Invisibles: Collection of Artificial Intelligence Generated Shorts},
  url = {https://doi.org/10.1145/3678698.3687210},
