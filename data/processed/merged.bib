  doi = {10.1145/3719160.3728626},
  isbn = {9798400715273},
  numpages = {5},
  publisher = {Association for Computing Machinery},
  series = {CUI '25},
  title = {Designing Age-Inclusive Interfaces: Emerging Conversational and Generative AI to Support Interactions across the Life Span},
  url = {https://doi.org/10.1145/3719160.3728626},
  year = {2025}
}

@inproceedings{merged1292,
  abstract = {With the advent of the intelligent era, the emergence of big data, cloud computing, artificial intelligence, etc. has brought earth-shaking changes to people's lives and production, and also provided technical support for the transformation and upgrading of the logistics and supply chain fields, greatly improving the operational efficiency of China's logistics and supply chain industries. This article starts from the definition and basic design framework of artificial intelligence, deeply studies the specific applications of artificial intelligence in the field of supply chain logistics, analyzes its application limitations, and proposes the future development trend of artificial intelligence in the field of supply chain logistics. It is hoped to play a certain reference role in improving the development of China's supply chain logistics.},
