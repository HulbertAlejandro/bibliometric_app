@inproceedings{merged1295,
  abstract = {With the rapid development and widespread application of artificial intelligence technologies, the global employment landscape is undergoing profound changes. The demand for low-skilled jobs is decreasing, while the demand for high-skilled jobs is increasing. This study selects labour market data for major economies from 2015 to 2024, and applies regression and cluster analysis to explore the intrinsic link between the degree of technology penetration and labour restructuring.The study finds that AI technologies have replaced around a quarter of low-skilled jobs in manufacturing, while at the same time high-skilled jobs such as data science and AI development have grown rapidly, further widening the skills gap. Moreover, the impact has varied significantly across countries and industries. Developed countries have experienced a massive loss of low-skill jobs and a dramatic increase in high-skill jobs, while developing countries have experienced similar trends, but with relatively smaller changes.Based on the above findings, the study makes a series of targeted policy recommendations, including strengthening vocational training, promoting industrial upgrading, improving the social security system, and enhancing the adaptability of the labour force, with the aim of mitigating the impact of AI on the employment structure and promoting the smooth development of the labour market.},
  address = {New York, NY, USA},
  author = {Li, Jingchun and Dong, Shizhu},
  booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area International Conference on Digital Economy and Artificial Intelligence},
  doi = {10.1145/3745238.3745508},
  isbn = {9798400712791},
  keywords = {Artificial Intelligence; Clustering Analysis; Employment Structure; Regression Analysis},
  numpages = {7},
  pages = {1722–1728},
  publisher = {Association for Computing Machinery},
  series = {DEAI '25},
  title = {The Dual Effects of Artificial Intelligence on Employment Structure: An Empirical Study Based on Regression and Clustering Models},
  url = {https://doi.org/10.1145/3745238.3745508},
  year = {2025}
}

@inproceedings{merged1296,
  abstract = {Audio-visual speech synthesis (AVSS) is a rapidly gaining prominence in artificial intelligence (AI) due to its widespread applications. AVSS refers to generating the audio-visual speech of a target speaker from the audio input of a source speaker, while preserving the speech content. The sequential steps of the AVSS process include voice conversion (VC) for altering vocal characteristics from the source to the target speaker and audio-visual synthesis (AVS) to generate a video stream from the converted speech of the target speaker. However, with the progressive development of deep generative models, the domain of AVSS remains relatively unexplored. The existing literature predominantly focuses on autoencoder-based AVSS models, potentially limiting their ability to capture the diversity of the vocal features and also synthesize the realistic video output. This paper introduces a novel AVSS approach utilizing a generative adversarial network (GAN) model, enriched with a capsule network (Caps-Net) architecture to effectively capture relevant input speech features and produce more natural audio-visual output. The proposed framework is trained and tested on VoxCeleb2 and LRS3-TED datasets and the experimental outcomes exhibit the superiority of the proposed approach over the state-of-the-art (SOTA) models.},
  address = {New York, NY, USA},
  author = {Ghosh, Subhayu and Jana, Nanda Dulal},
  booktitle = {Proceedings of the 8th International Conference on Data Science and Management of Data (12th ACM IKDD CODS and 30th COMAD)},
  doi = {10.1145/3703323.3703325},
  isbn = {9798400711244},
  keywords = {Audio-Visual Speech Synthesis (AVSS); Audio-Visual Synthesis (AVS); Capsule Network (Caps-Net).; Generative Adversarial Network (GAN); Voice Conversion (VC)},
  numpages = {9},
  pages = {10–18},
  publisher = {Association for Computing Machinery},
  series = {CODS-COMAD '24},
  title = {Audio-Visual Speech Synthesis Leveraging Capsule-Enhanced Generative Adversarial Network},
  url = {https://doi.org/10.1145/3703323.3703325},
  year = {2025}
}

@inproceedings{merged1297,
  abstract = {Empowering vocational education with artificial intelligence (AI) is a new development direction for vocational education in the new era. Universities and enterprises are two important entities in the research and development (R&amp;D) activities of empowering vocational education with AI. Under the condition of bounded rationality, this paper constructs a university-enterprise evolutionary game model, studies the game interaction process between the two parties in different situations, and verifies the evolutionarily stable strategies through numerical simulation analysis. The results show that: The difference between supervision cost and non-supervision cost of universities, and the difference between high-quality R&amp;D cost and low-quality R&amp;D cost of enterprises are the key factors affecting the evolutionarily stable strategies of universities and enterprises respectively; Strengthening supervision of AI projects by government departments and all sectors of society can effectively promote supervision of universities and high-quality R&amp;D of enterprises; It is also an effective incentive method for universities to appropriately increase the government incentives allocated to enterprises.},
  address = {New York, NY, USA},
  author = {Fang, Wenchao},
  booktitle = {Proceedings of the 2024 8th International Conference on Computer Science and Artificial Intelligence},
  doi = {10.1145/3709026.3709108},
  isbn = {9798400718182},
  keywords = {Artificial intelligence; Enterprise; Evolutionary game; University; Vocational education},
  numpages = {10},
  pages = {456–465},
  publisher = {Association for Computing Machinery},
  series = {CSAI '24},
  title = {Evolutionary Game Analysis of Universities and Enterprises in Empowering Vocational Education with Artificial Intelligence},
  url = {https://doi.org/10.1145/3709026.3709108},
  year = {2025}
}

@inbook{merged1298,
  abstract = {This study utilized CiteSpace to perform an extensive knowledge mapping analysis of 826 Chinese articles and 488 international publications on artificial intelligence in education, obtained from the China National Knowledge Infrastructure (CNKI) and Web of Science databases, spanning the years 2014 to 2024. The research employed visualization methods, such as co-citation analysis and network algorithms, to comprehensively assess research hotspots, key topics, and emerging trends in the field, both domestically and internationally. Research indicates that overseas studies prioritize the incorporation of AI technology in education and the development of theoretical frameworks, whereas domestic research focuses on the practical use of AI in curriculum development and educational administration. Despite these divergent perspectives, both international and domestic research entities share a mutual interest in the use and advancement of AI in education, especially in relation to personalized learning and educational equity. To promote AI in global education, Future research should prioritize AI-driven digital transformation and standardization in education, Interdisciplinary collaboration and leveraging big data analytics will enhance innovation and decision-making, Addressing ethical issues, privacy, and societal acceptance is crucial for equitable and sustainable educational development.},
  address = {New York, NY, USA},
  author = {Pan, Shengwu and Zeng, Qi and Li, Mingjiang and Yan, Na},
  booktitle = {Proceedings of the 4th Asia-Pacific Artificial Intelligence and Big Data Forum},
  isbn = {9798400710865},
  numpages = {12},
  pages = {404–415},
  publisher = {Association for Computing Machinery},
  title = {Global and Domestic Perspectives on AI in Education: A Knowledge Mapping Analysis Using CiteSpace},
  url = {https://doi.org/10.1145/3718491.3718558},
  year = {2025}
}

@inproceedings{merged1299,
  abstract = {Artificial Intelligence (AI) has become a fundamental tool for cybersecurity researchers and practitioners. It is frequently used to address major security problems such as supply chain attacks, ransomware threats, and social engineering. In this context, integrating AI into cybersecurity workflows requires incorporating AI-driven approaches into the educational training of the cybersecurity workforce. This paradigm shift in academic settings will introduce the necessary skills for cybersecurity professionals to operate modern AI-based systems. Yet, the current cybersecurity curriculum still suffers from the absence of AI resources, particularly the detailed understanding of the appropriate AI mechanisms. Such absence leaves skill gaps for future professionals and practitioners in the industry. To address this, we designed an academic lecture module on AI covering both theory and practice. Then, we taught the module across six cybersecurity courses in our institution. To assess the effectiveness of integrating AI materials into cybersecurity education, we collected data by presenting two surveys before and after the lecture (concluding 81 participants per survey). Specifically, we utilized widely accepted models for unbiased analysis of our data. Our experimental results show positive AI knowledge improvement by 30% of the participants, demonstrating the beneficial impact of the lecture. Then, we observed a high similarity score between the survey responses and the lecture content, reaching 84%. Moreover, our sentiment analysis results reflect positive feedback from the participants with a positive score of 0.50. Overall, our study serves as a reference for instructional designers for developing educational curricula aiming to integrate AI into cybersecurity education.},
  address = {New York, NY, USA},
  author = {Brito, Fernando and Mekdad, Yassine and Ross, Monique and Finlayson, Mark A. and Uluagac, Selcuk},
  booktitle = {Proceedings of the 56th ACM Technical Symposium on Computer Science Education V. 1},
  doi = {10.1145/3641554.3701958},
  isbn = {9798400705311},
  keywords = {artificial intelligence; cybersecurity; education; integration; machine learning; natural language processing},
  location = {Pittsburgh, PA, USA},
  numpages = {7},
  pages = {158–164},
  publisher = {Association for Computing Machinery},
  series = {SIGCSETS 2025},
  title = {Enhancing Cybersecurity Education with Artificial Intelligence Content},
  url = {https://doi.org/10.1145/3641554.3701958},
  year = {2025}
}

@inbook{merged1300,
  abstract = {In the past decade, chorus as a form of musical art performance, in the more developed areas of China, young people interested in chorus and retired elderly people have joined the choir for learning. This paper seeks to understand the problems of teaching choral tunes in a way that the singing voice is not easy to learn and the students are not easy to understand the instructor's verbal comments and corrections, and proposes the use of artificial intelligence technology to assist the teaching of choral tunes and to solve the above problems. This paper proposes an auxiliary system for choral teaching, for which this study collects a collection of a cappella vocal recordings of a choral seven-word tune, and uses music information retrieval and machine learning algorithms to analyze and compare the recording files, and outputs style similarity scores and visualization charts of the user's input and the collection of sound files. Teachers of choral teaching can use this system to solve the difficulties in teaching choral promotion, and the karaoke game-like tune teaching system has a higher willingness to learn for choral beginners.},
  address = {New York, NY, USA},
  author = {Xu, Yang},
  booktitle = {Proceedings of the 2024 2nd International Conference on Information Education and Artificial Intelligence},
  isbn = {9798400711732},
  numpages = {7},
  pages = {469–475},
  publisher = {Association for Computing Machinery},
  title = {Applying Artificial Intelligence Techniques to Build a Singing Evaluation System in Choral Singing},
  url = {https://doi.org/10.1145/3724504.3724582},
