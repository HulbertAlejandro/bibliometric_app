  numpages = {5},
  pages = {396–400},
  publisher = {Association for Computing Machinery},
  series = {DIS '24 Companion},
  title = {Death of the Design Researcher? Creating Knowledge Resources for Designers Using Generative AI},
  url = {https://doi.org/10.1145/3656156.3658398},
  year = {2024}
}

@inproceedings{merged629,
  abstract = {Private Set Intersection Cardinality (PSI - CA) is a cryptographic primitive through which two or more participants can obtain the intersection cardinality of their private sets and the privacy of the sets is protected. In this paper, we propose a tripartite protocol that utilizes quantum resources to accomplish these tasks, in which each two or more parties can obtain the intersection cardinality of their private sets with the help of semi-honest third parties (TP). Using this property, a voting protocol is designed. In our protocol, the GHZ state is used to encode private information that TP will use to compute the cardinality. We prove that the proposed protocol is secure against well-known quantum attacks. Furthermore, compared with existing quantum voting schemes, the protocol achieves higher efficiency.},
  address = {New York, NY, USA},
  author = {Zheng, Yifan and Liu, B.},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3665348.3665390},
  isbn = {9798400709562},
  location = {Kuala Lumpur, Malaysia},
  numpages = {6},
  pages = {243–248},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '24},
  title = {A secure multi-quantum voting protocol based on GHZ state},
  url = {https://doi.org/10.1145/3665348.3665390},
  year = {2024}
}

@article{merged630,
  abstract = {The rapid progression of generative artificial intelligence (GenAI) tools has raised significant interest and concern in academia. Instances of students submitting AI-generated assignments prompt investigations into implications for teaching, learning, and academic integrity. Recent publications highlight concerns such as a lack of conceptual understanding, threats to academic integrity, and disruptions to traditional assessment methods. While recognizing benefits like automated scoring and personalized learning, authors stress the responsible use of GenAI, emphasizing the educator's role in guiding students. This commentary identifies opportunities and threats of GenAI in African university contexts. Opportunities include increased operational efficiency, content generation, automated assessment, recognition of accessibility needs, overcoming language barriers, and accelerated research. However, these tools require human correction and cautious consideration of job displacement concerns. Threats encompass job displacement, privacy and security issues, threats to academic integrity, hallucinations/confabulations of GenAI, access and infrastructure challenges, technological overemphasis, lack of customization for local needs and cultural contexts, dependency on external providers, and unaffordable costs. The need for robust guidelines that balance technological advances with traditional teaching methods in African universities is emphasized. Given digital transformation initiatives like the African Union's Agenda 2063 and Botswana's SmartBots strategy, integrating GenAI could shape the future of African tertiary education. Proactive policies should address ethical concerns, ensure access, and make GenAI tools available, requiring a collaborative effort to navigate its impact responsibly.},
  address = {New York, NY, USA},
  articleno = {18},
  author = {Zlotnikova, Irina and Hlomani, Hlomani},
  doi = {10.1145/3689215},
  issue_date = {June 2025},
  journal = {Digit. Gov.: Res. Pract.},
  keywords = {African countries; Generative artificial intelligence; universities},
  month = {June},
  number = {2},
  numpages = {11},
  publisher = {Association for Computing Machinery},
  title = {GenAI in the Context of African Universities: A Crisis of Tertiary Education or Its New Dawn?},
  url = {https://doi.org/10.1145/3689215},
  volume = {6},
  year = {2025}
}

@inproceedings{merged631,
  abstract = {With the rapid development of big data and artificial intelligence technology, recommendation systems play a crucial role in improving user experience, increasing platform revenue, and optimizing content distribution. However, the wide application of recommendation system also brings about problems such as filter bubble, algorithm black box, labor rights protection, the big data-enabled price discrimination and so on. By processing massive data, the large language model can accurately construct and predict user interests and preferences, process and generate diverse content, thus improving the relevance and fairness of recommendation, enhancing the diversity and transparency of recommendation system, protecting the rights and interests of workers, preventing price discrimination, and realizing algorithm governance and the protection of user rights and interests. This paper designs a recommendation algorithm framework based on a large language model, aiming to build a fair and transparent recommendation system, which can reduce information bias, improve the transparency of the list, protect the rights and interests of workers, prevent price discrimination, and promote social equity.},
  address = {New York, NY, USA},
  author = {Wei, Han and Qu, Dan and Zhang, Wenlin and Niu, Tong and Peng, Sisi and Guo, Zhigang},
  booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
  doi = {10.1145/3734921.3734930},
  isbn = {9798400713460},
  keywords = {Algorithm governance; Large language model; Recommendation algorithm; User rights and interests},
  numpages = {6},
  pages = {52–57},
  publisher = {Association for Computing Machinery},
  series = {GADM '25},
  title = {Recommendation Algorithm Based on Large Language Model: Consideration on Algorithm Governance and User Rights Protection},
  url = {https://doi.org/10.1145/3734921.3734930},
  year = {2025}
}

@article{merged632,
  abstract = {In this article, we propose (re)designing privacy literacy as an essential component of our digital lives in an age of Generative Artificial Intelligence (genAI). Our study emphasizes the layered digital, technical, rhetorical, and algorithmic literacies associated with design thinking and genAI to support theorizing privacy literacy. We introduce Design as an analytical element complementary to Woods and Wason's (2021) multi-pronged framework for analyzing Terms of Service (ToS) documents. Using a cluster of Adobe Generative AI ToS, we illustrate the necessity of including Design, which allows those invested in Communication Design (CD) and Technical and Professional Communication (TPC) to interrogate how or if design supports or undermines values related to user privacy, data ownership, and informed consent. We conclude by detailing how collective surveillance apathy regarding emergent data infrastructures signal a Post-Surveillance era in our global society and digital lives.},
  address = {New York, NY, USA},
  author = {Woods, Charles and Johnson, Gavin P.},
  doi = {10.1145/3655727.3655736},
  issue_date = {June 2024},
  journal = {Commun. Des. Q. Rev},
  keywords = {artificial intelligence; design; digital privacy; post-surveillance; terms of service},
  month = {January},
  number = {2},
  numpages = {12},
  pages = {86–97},
  publisher = {Association for Computing Machinery},
  title = {(Re)Designing Privacy Literacy in the Age of Generative AI},
  url = {https://doi.org/10.1145/3655727.3655736},
  volume = {12},
  year = {2025}
}

@inbook{merged633,
  abstract = {Transformer have been widely used in the field of natural language processing due to their excellent performance, but the high computational cost and large parameter scale limit the efficiency of actual deployment. As an effective model compression technique, model distillation constructs small-scale student models from large models through knowledge transfer, significantly reducing resource requirements. However, there is a risk of information leakage during the distillation process of the model. This article deeply analyzes the potential mechanisms of information leakage in the distillation process of Transformer, including data reproduction attacks, reverse inference attacks, and other issues. In response to the above risks, a method combining differential privacy technology with secure multi-party computation was proposed for protection, and its effectiveness was verified through experiments. Research has shown that this method can significantly reduce the risk of information leakage while ensuring model performance.},
  address = {New York, NY, USA},
  author = {Wei, Letian and Zhang, Zhiman and Zheng, Junhua},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {7},
  pages = {43–49},
  publisher = {Association for Computing Machinery},
  title = {Privacy distillation: Collaborative privacy protection for Transformer},
  url = {https://doi.org/10.1145/3728725.3728733},
  year = {2025}
}

@inproceedings{merged634,
  abstract = {In practical application environments, the task of piano music transcription confronts a series of challenges including the long-tail distribution characteristics of training data, inadequate transcription accuracy in multi-key scenarios, interference from environmental noise, and the inability of mobile devices to meet the demands of real-time transcription. This study builds upon the Onsets and Frames model and introduces a series of structural improvements aimed at enhancing model performance. These include the simplification and optimization of the model structure, the introduction of data augmentation strategies, and the refinement of the loss function. Precise quantization of the model, coupled with effective mobile deployment strategies, ensures the efficient operation of the model on mobile devices. Experimental validation in the mobile environment demonstrates that the proposed improvements significantly enhance transcription accuracy in various scenarios, achieving a 56.98% increase in transcription speed compared to the original model, thereby meeting the requirements for real-time transcription. Moreover, the new model exhibits substantial noise resistance capabilities, particularly in low signal-to-noise ratio conditions where the performance degradation is considerably less than that of the original model.},
  address = {New York, NY, USA},
  author = {Wan, Pengcheng and Hu, Wei},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3665348.3665415},
  isbn = {9798400709562},
  location = {Kuala Lumpur, Malaysia},
  numpages = {6},
  pages = {394–399},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '24},
  title = {Real-time recognition of piano keys based on mobile devices},
  url = {https://doi.org/10.1145/3665348.3665415},
  year = {2024}
}

@article{merged635,
  abstract = {This application paper introduces a transformative solution to address the labour-intensive manual report generation, data searching and report revision process in government entities. Traditional methods of data extraction, analysis, and graph creation for annual reports are not only time-consuming but also prone to human errors. To mitigate these challenges, we propose an innovative system leveraging Generative Artificial Intelligence (GenAI), with a specific focus on large language models (LLMs). Our solution incorporates automated data extraction from diverse sources designated as internal knowledge base, text analysis, summarization using advanced language models, and the generation as well as revisions of informative graphs. Different LLMs like Google's Gemini Pro and OpenAI's GPT 4.0 have been used to read different data visualisation graphs and fetch the information from the internal knowledge base, respectively, to update the graphs in an automated manner. Solution implementation using Python language on the World Economic Situation and Prospects report by the Department of Economic &amp; Social Affairs, United Nation shows that the early result produces almost 0.87% to 17.46% average error rates in the task of factual data visualisation graph. Key benefits of this approach include improved time efficiency, consistency in report format, enhanced insights, and a user-friendly interface. A review and approval workflow facilitate user feedback, contributing to continuous model performance improvement.},
  address = {New York, NY, USA},
  articleno = {4},
  author = {Gupta, Rajan and Pandey, Gaurav and Pal, Saibal Kumar},
  doi = {10.1145/3691352},
  issue_date = {March 2025},
  journal = {Digit. Gov.: Res. Pract.},
  keywords = {GPT; Gemini Pro; Generative AI; OpenAI; automated graph revision; data fetching; data generation; data visualisation; knowledge update},
  month = {February},
  number = {1},
  numpages = {10},
  publisher = {Association for Computing Machinery},
  title = {Automating Government Report Generation: A Generative AI Approach for Efficient Data Extraction, Analysis, and Visualization},
  url = {https://doi.org/10.1145/3691352},
  volume = {6},
  year = {2025}
}

@inproceedings{merged636,
  abstract = {Leading the innovative development of higher education by digitization of higher education and exploring a road of education digitization development with Chinese characteristics is a necessary way for our country to realize the construction of a powerful country in education. This study builds online learning environment with the help of generative artificial intelligence tools and platforms, relying on the public course “Modern Educational Technology” for normal university students and integrating ideological and political education into the curriculum, guided by human-machine co-creation projects, independent learning activities, teacher-student interaction activities, and student-student interaction activities are designed to improve the information-based instructional design and information-based resource development capabilities of normal university students and promote deep learning. The results of the study show that online interactive activities for deep learning can improve students' deep learning ability, digital literacy and enhance the effect of online interaction. At the same time, this study also proposes relevant practical measures in order to provide a reference for online teaching in the era of generative artificial intelligence.},
