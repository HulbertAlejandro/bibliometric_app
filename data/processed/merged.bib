@inbook{merged760,
  abstract = {The rapid development of computer technology has brought computer graphics a qualitative leap in graphic images and visual communication. Computer graphics transforms digital information into image information while conducting digital processing and visualization of graphic images. Combining with the development process of computer graphics, this paper considers the application field of computer graphics, discusses the connotation deduction of multi-modal translation resource. Through case analysis of the Visionbank, this paper validates and advances computer graphics technology to optimize multi-modal translation resource, and realizes the comprehensive utilization of multi-modal translation resource.},
  address = {New York, NY, USA},
  author = {Xu, Xintao},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {5},
  pages = {654–658},
  publisher = {Association for Computing Machinery},
  title = {Exploration of Multi-modal Translation Resource Processing Modes under Computer Graphics: A Case Study of Visionbank},
  url = {https://doi.org/10.1145/3728725.3728828},
  year = {2025}
}

@inbook{merged761,
  abstract = {In recent years, the quantitative analysis of the green quality of urban streets has become a hot spot in the cross-research of computer vision and urban environment. In this paper, we propose a semantic segmentation method based on a dynamic multimodal noise addition technique combined with the SegFormer model to improve the robustness of street view image segmentation in complex environments. The study takes five major roads (West Beijing Road, North Taiping Road, Changjiang Road, Yihe Road, and Central Road) in Nanjing as objects, and based on the streetscape and remote sensing data of the vegetation growth season from 2017 to 2022, the Gaussian noise, motion blur, and weather effects are simulated by the multimodal noise addition technique, which effectively improves the adaptability of the model to the dynamic interference. The experiments showed that the Green View Index analysis revealed a general upward trend of street greening (average increase of 0.94%). Furthermore, the Kendall rank correlation analysis reveals the positive correlation between environmental factors and green visualization rate, and the influence law of environmental factors on green perception is verified. This study provides a high-precision computer vision analysis framework for urban greening planning and promotes the application of semantic segmentation models in complex urban scenes.},
  address = {New York, NY, USA},
  author = {Liu, Yang and Jin, Guanghu and Zhang, Jinji and Xue, Xiyue},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {6},
  pages = {480–485},
  publisher = {Association for Computing Machinery},
  title = {Green quality perception of urban streets: a study combining semantic segmentation and multimodal noise enhancement techniques},
  url = {https://doi.org/10.1145/3728725.3728800},
  year = {2025}
}

@inbook{merged762,
  abstract = {An adaptive fast robust scaled consensus tracking problem for multi-agent systems (MASs) with unknown dynamics is studied in this paper under a directed graph. Each agent's dynamic is described by a high-order differential equation, which includes time delays and uncertainty. A novel fully distributed adaptive control method is proposed to ensure that all following agents track the leader with prescribed scaled ratios asymptotically, and the tracking errors are uniform ultimate boundedness. Meantime the proposed approach guarantees the bounded adaptive estimates of the unknown parameters and dynamics, and can lighten the burden on computation because it only needs to update a small number of adaptive parameters. The control gain of each agent is heterogeneous and time-varying. It changes and updates by the designed distributed adaptive law. The presented protocol doesn't need to know global nonzero eigenvalues of communication graph adjacency matrix, and is independent of any global information. Finally, the effectiveness of the theoretical results is verified by a numerical example.},
  address = {New York, NY, USA},
  author = {Zhang, Zheng and Xia, Jiewen and Yu, Yang},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {7},
  pages = {491–497},
  publisher = {Association for Computing Machinery},
  title = {Fully distributed scaled consensus tracking of high-order unknown nonlinear multi-agent systems},
  url = {https://doi.org/10.1145/3728725.3728802},
  year = {2025}
}

@inbook{merged763,
  abstract = {Pre-trained language models based on deep neural networks have achieved superior performance in Chinese spelling correction. However, these models are heavily dependent on the size and quality of the training corpus and exhibit poor performance when faced with unknown errors. Inspired by adversarial learning, this paper focuses on Chinese character input scenarios and propose an effective attack strategy to generate more valuable training examples in these scenarios. This strategy jointly utilizes the attention mechanism and the logit output to determine the attack position and generates misspelling instances in a perturbed pinyin manner. The generated adversarial samples are then gradually added to the training set to train the model. Further, this paper explores the effect of the proportion of samples in the adversarial training process on model performance. The effectiveness of the proposed strategy are demonstrated on three different architectural baseline models.},
  address = {New York, NY, USA},
  author = {Wang, Xiaoyu and Zhao, Weidong and Qiu, Liqing},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {8},
  pages = {50–57},
  publisher = {Association for Computing Machinery},
  title = {Building Specialized Adversarial Examples for Improved Spelling Error Correction},
  url = {https://doi.org/10.1145/3728725.3728734},
  year = {2025}
}

@inbook{merged764,
  abstract = {This paper aims to design a chatbot application to help people find an appropriate restaurant based on users' preferences and transportation modes. It also aims to explore whether a machine learning-based chat agent is capable of accomplishing all the tasks precisely, or whether it could be optimized by collaborating with other simple agents. This paper establishes a chatbot environment along with a series of agents (machine learning agent, grammar checker agent, spelling checker agent, restaurant searcher), designed to assist users in finding a restaurant mainly based on what transportation they use and if a restaurant is found, the chatbot could display the route to this place in the prompt also with map details by employing the Google map API. The machine learning agent acting as the brain of this chatbot, could not only be able to evoke other agents corresponding to users’ input but also could directly store what kinds of transportation are taken by users and pass to Google map API to search appropriate restaurants instead of fully relying on a restaurant searcher agent (rule-based agent). To examine whether multiple agents coordinate with each other could outperform a single agent. A contrast experiment is conducted to conclude the result, which involves 15 participants who run through this program under two different setups (with and without spelling and grammar checkers) and each of them would give a query to this chatbot but this query would be dealt with and without spelling and grammar checker respectively. The result would be quantitatively evaluated by two metrics: Success Rate: successfully finding a restaurant and showing the map; the Total of Queries. Results suggest that pre-processing input with spelling and grammar checkers significantly improves the chatbot's performance, achieving an 80% success rate (12 out of 15 queries). In contrast, when input is not pre-processed, the success rate drops to 46.66% (7 out of 15 queries). This indicates that the involvement of spelling and grammar checker agents enhances the accuracy and efficiency of restaurant searches, making the collaborative multi-agent setup more effective. Moreover, it could demonstrate that this chatbot environment is well constructed since it could successfully provide suitable restaurants given users’ requirements.},
  address = {New York, NY, USA},
  author = {Lan, Hongxu},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {8},
  pages = {427–434},
  publisher = {Association for Computing Machinery},
  title = {Comparing the accuracy and efficiency of a single machine learning agent with multiple coordinating agents in a chat environment},
  url = {https://doi.org/10.1145/3728725.3728792},
  year = {2025}
}

@inbook{merged765,
  abstract = {We investigate whether Large Language Models can synthesize correct network configuration templates in a simpler and more efficient way with limited prompts. The experimental results demonstrate that the LLM's shortcomings in command relation understanding can be supplemented by external models to generate promising templates. We do not rule out the contribution of validators to property-specific consistency. However, our framework aims to preserve the large language model generation capability while focusing on the implicit relationships in configurations. By introducing a hierarchical command graph, we follow a top-down matching to generate templates, reducing feedback interactions in generation and bring potential solutions for automated provisioning. We process template generation under different scenarios, including establishing BGP basic connections on multiple routers and imposing restrictions such as route redistribution at specific locations. Human involvement is restricted to providing critical information. Experiment shows that expected templates can be generated in the first iteration, particularly for the basic task. The advanced task is influenced by multiple factors, however, it still shows considerable potential in terms of coverage with possibly feasible templates.},
  address = {New York, NY, USA},
  author = {Zhang, Xiaofeng and Gao, Xianming and Tao, Peilin and Feng, Tao},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {7},
  pages = {108–114},
  publisher = {Association for Computing Machinery},
  title = {GraphSynth: Synthesis of Network Configuration Templates Using Large Language Models},
  url = {https://doi.org/10.1145/3728725.3728742},
  year = {2025}
}

@inbook{merged766,
  abstract = {With the wide deployment of deep learning models in various applications, the reliability problem of neural networks has gradually become a research hotspot. Computer systems are susceptible to single-particle effects, electromagnetic interference, and other factors in complex environments, resulting in frequent bit-flipping faults. Among them, Silent Data Corruption (SDC), as an imperceptible fault, cannot be detected by the system, which ultimately destroys the output results of neural network models. Current SDC detection techniques mainly focus on the system instruction stream level, and lack efficient methods to implement detection reinforcement at the model level. In addition, due to the black-box weight update mechanism of neural networks, traditional selective redundancy methods cannot provide a reliable explanation of which important weights to fix. To address these issues, this paper proposes a Convolutional Neural Network Reinforcement Framework Based on Neural Network Interpretability (RFNI). RFNI explores the redundancy ratio of convolutional kernels by performing layer-by-layer search in convolutional neural networks through SAC (Soft Actor-Critic) agent; secondly, the importance of each convolutional kernel in the current layer is calculated by combining with neural network interpretability algorithms; and finally, the importance of each convolutional kernel in the current layer is calculated based on the total Floating Point Operations Per Second (FLOPS). Second, the importance of each convolutional kernel in the current layer is calculated by combining with the neural network interpretable algorithm; finally, the granularity configurable redundancy reinforcement is implemented based on the total floating point operations per second (FLOPS). The experimental results show that RFNI on multiple neural network architectures significantly improves the recognition accuracy in case of SDC over existing methods, and can effectively enhance the robustness of the model.},
  address = {New York, NY, USA},
  author = {Song, Haocheng and Yu, Pengfei and Ou, Jiayi and Li, Wei and Gu, Jingjing},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {6},
  pages = {585–590},
  publisher = {Association for Computing Machinery},
  title = {A Convolutional Neural Network Reinforcement Framework Based on Neural Network Interpretability},
  url = {https://doi.org/10.1145/3728725.3728817},
  year = {2025}
}

@inbook{merged767,
  abstract = {In this study, hyperspectral image reconstruction of Dangshan pear was realized based on SSR technique by extracting different number of channels from the original HSI images as input to the SSR model. Subsequently, the spectra of 79 important wavelengths obtained by CARS algorithm was reconstructed by proposed lightweight SSR model at the optimal number of input channels. The model built by partial least squares (PLS) and reconstructed spectra is used to predict the SSC of pears with a Rp2 of 0.668 and EMSEp of 0.366. The experimental results show that increasing the input channels can improve the reconstruction quality of the SSR model, and the reasonable selection of output channels helps to remove the redundant information and realize the accurate prediction of SSC, which illustrated that the importance of rationally selecting the input channels of the SSR model as well as the output channels to the SSR results.},
  address = {New York, NY, USA},
  author = {Hu, Dan and Zheng, Shouguo},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {8},
  pages = {374–381},
  publisher = {Association for Computing Machinery},
  title = {Estimation of Soluble Solids Content for Dangshan Pear using Hyperspectral Image Reconstruction Based on Deep Learning},
  url = {https://doi.org/10.1145/3728725.3728785},
  year = {2025}
}

@inbook{merged768,
  abstract = {An improved adaptive 3D ResNet-18 residual network gait recognition method is proposed for the problem of recognizing people's identity in videos captured by UAVs. Using a combination of gait feature extraction and deep learning models, human silhouettes are separated from complex backgrounds for modeling, and their gait features are extracted for identification.3D ResNet-18 residual network learns spatio-temporal features to extract temporal and spatial features in the sequence of video frames and introduces a cross-entropy loss function to optimize the model and improve the prediction accuracy. When dealing with the challenges of motion blurring in the video captured by drones and shadows and noise interference in background modeling, the improved MOG2 background separation algorithm detects pedestrian gait and models it, and adaptive variance thresholding improves the background detection capability and improves the spatio-temporal feature extraction and optimization, which significantly improves the robustness of the model. The experiments use the Walking category in the KTH public dataset, and the results show that the model exhibits good recognition results in different backgrounds. This approach effectively improves the performance of UAVs in applications such as security surveillance.},
  address = {New York, NY, USA},
  author = {Wang, Jingsi and Li, Xiaobin and Zhu, Pengyu and Chen, Zhuoyi},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {5},
  pages = {354–358},
  publisher = {Association for Computing Machinery},
  title = {Identification of personnel identity in unmanned aerial vehicle video gait recognition based on 3Dresidual network},
  url = {https://doi.org/10.1145/3728725.3728782},
  year = {2025}
}

@inbook{merged769,
  abstract = {Large-scale language models are critical for natural language processing, and efficient distributed learning is needed to train such models. In this paper, a distributed training scheme is developed to improve the efficiency and scalability of training large-scale language models. This training scheme is divided into a three-layer structure, and also incorporates an autotuned gradient compression method and a load balancing mechanism to improve training efficiency at any time. Experiments show that this scheme is highly efficient in terms of computation and resource utilization for large-scale training tasks, is much faster than current open systems, and offers new ideas for faster training of large-scale language models.},
  address = {New York, NY, USA},
  author = {Gong, Jiangchuan and Wang, Yang},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {6},
  pages = {551–556},
  publisher = {Association for Computing Machinery},
  title = {Research on Distributed Training Architecture for Large Scale Models for Natural Language Processing},
  url = {https://doi.org/10.1145/3728725.3728812},
  year = {2025}
}

@inbook{merged770,
  abstract = {Regular network traffic prediction techniques, which generally rely on data normalization, often fail to account for the non-stable characteristics of network traffic, leading to a decline in estimate reliability. A novel approach to this problem is the Trend Decomposition Enhanced Large Language Model for Non-Static Network Traffic Prediction (TD-LLM). Applying trend decomposition as the preparation approach, the proposed strategy first divides raw information into resolved and non-fixed parts. The predetermined portion is standardized and processed, implementing a multi-head cross-attention mechanism, while a better self-attention mechanism improves the non-stationary element. Prompt engineering is used to create specially designed causes that serve as a guideline for the design throughout the forecast process. Finally, the processed data and creates are integrated into the large language model to produce the predicted results. In contrast to foundation models like Time- LLM, LSTM, Informer, and Transformer, the TD-LLM type drastically reduces mean squared error for network traffic prediction across many regions, according to exploratory evaluations, with decreases of 19.73 %, 54.71 %, 76.88 %, and 61.48 %, respectively.},
  address = {New York, NY, USA},
  author = {Yang, Kai and Shi, Huaifeng and Wang, Rui and Zhou, Lei and Peng, Jiahui and Liu, Chaofan},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {8},
  pages = {12–19},
  publisher = {Association for Computing Machinery},
  title = {Trend Decomposition Enhanced Large Language Model for Non-Stationary Network Traffic Prediction},
  url = {https://doi.org/10.1145/3728725.3728728},
  year = {2025}
}

@inbook{merged771,
  abstract = {In recent years, the increasing number of vehicles has led to heightened traffic congestion. Traditional surveillance is limited to monitoring fixed locations. However, unmanned aerial vehicle (UAV) photography provides advantages such as convenience and a wide perspective, gradually replacing these conventional techniques for traffic video collection. Nonetheless, challenges including complex scenes, varying scales and frequent occlusions result in issues such as false positives, missed targets, and identity switches (IDSW). To tackle these, we developed the UVD detection algorithm based on YOLOv8-n and the AByteTrack tracking algorithm derived from ByteTrack. The integration of these two components forms the UVD-ATrack algorithm. In the backbone of the detection section, we introduced the RFAConv and designed RF_C2f to enhance feature extraction capabilities. For the neck component, the adaptive weight fusion module (AWFM) was developed to effectively combine multi-scale local information. During the tracking phase, we optimized the motion model. Additionally, camera motion compensation (CMC) was implemented to adaptively mitigate the effects of sharp perspective changes. Test results utilizing the Visdrone dataset indicate that mAP@0.5 and mAP@0.5:0.95 improved by 4.4% and 3.6% respectively in the detection section. In the tracking test, IDF1, HOTA, MOTA and MOTP also exhibited significant enhancement while IDSW decreased by 77%. Compared with state-of-the-art methods, our proposed algorithm demonstrates exceptional performance.},
  address = {New York, NY, USA},
  author = {Li, Chengxuan and Yang, Wei and Sun, Chang},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {7},
  pages = {673–679},
  publisher = {Association for Computing Machinery},
  title = {UVD-ATrack: UAV-based Vehicle Detection and Adaptive Tracking},
  url = {https://doi.org/10.1145/3728725.3728831},
  year = {2025}
}

@inbook{merged772,
  abstract = {Pneumonia, a severe respiratory infection, is commonly caused by either viral or bacterial pathogens. Timely identification of pneumonia is essential for effective patient care and treatment. Chest X-ray imaging is widely utilized for both diagnosing and detecting pneumonia, though interpreting these images presents certain difficulties. This research leverages transfer learning to evaluate and compare the performance of various convolutional neural networks (CNNs) in the context of chest X-ray image classification and detection. The results of the experiment demonstrate that the lightweight ShuffleNetV2 model excels in minimizing computational burden and memory consumption, while preserving high accuracy and effectiveness in classification tasks.},
  address = {New York, NY, USA},
  author = {Wu, Chunyang},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {5},
  pages = {621–625},
  publisher = {Association for Computing Machinery},
  title = {Pneumonia Detection and Classification Using Lightweight ShuffleNetV2 Based on Transfer Learning},
  url = {https://doi.org/10.1145/3728725.3728823},
  year = {2025}
}

@inbook{merged773,
  abstract = {The pervasiveness of the dissemination of fake news through social media platforms poses critical risks to the trust of the general public, societal stability, and democratic institutions. This challenge calls for novel methodologies in detection, which can keep pace with the dynamic and multi-modal nature of misinformation. Recent works include powering the detection using large language model advances in multimodal frameworks, methodologies using graphs, and adversarial training in the literature of fake news. Based on the different approaches which can bring success, some key highlights will be underlined: enhanced LLM-improves accuracy through more advanced semantics and cross-modality fusion for robust detections. The review further identifies critical gaps in adaptability to dynamic social media trends, real-time, and cross-platform detection capabilities. Future directions underline the development of style-agnostic models, cross-lingual detection frameworks, and robust policies with a view to mitigating LLM-driven misinformation. This synthesis thus lays a concrete foundation for those researchers and practitioners committed to reinforcing fake news detection systems with complications that keep on growing in the digital landscape.},
  address = {New York, NY, USA},
  author = {Yi, Jingyuan and Xu, Zeqiu and Huang, Tianyi and Yu, Peiyang},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {7},
  pages = {87–93},
  publisher = {Association for Computing Machinery},
  title = {Challenges and Innovations in LLM-Powered Fake News Detection: A Synthesis of Approaches and Future Directions},
  url = {https://doi.org/10.1145/3728725.3728739},
  year = {2025}
}

@inbook{merged774,
  abstract = {Session-based recommendation systems predict what a user might be interested in next based on their current behavior. However, many existing methods face issues, such as insufficient use of overall context and difficulty in extracting key features due to noise interference. To address these challenges, this paper introduces a model called GC-DMRec. This model combines global context with gated slot attention mechanisms, effectively integrating local session graphs with global context graphs to track item changes both within and across sessions. By using gated slot attention, GC-DMRec focuses on critical time points, extracting more representative features while reducing noise. Experimental results on datasets such as Diginetica, Tmall, and Nowplaying demonstrate that GC-DMRec outperforms other existing methods in terms of accuracy and reliability.},
  address = {New York, NY, USA},
  author = {Luo, Lei and Zhan, Hualin and Jiang, Weinan and Zhang, Ziqian},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {6},
  pages = {667–672},
  publisher = {Association for Computing Machinery},
  title = {Enhanced Session-Based Recommendation with Global Context and Dynamic Memory},
  url = {https://doi.org/10.1145/3728725.3728830},
  year = {2025}
}

@inbook{merged775,
  abstract = {In factories, fans is an important part in production. However, how to ensure the safe and stable operation of it is always a difficult problem. In order to solve this problem, this study presents a fan anomaly detection method based on an improved CBAM attention mechanism using the MobileNetV3-Small model. In this model, the conventional SE module is replaced with an optimized CBAM, which uses dilated convolutions to enhance the model's sensitivity to anomalous sounds. At the same time, the model incorporates CutMix and Focal Loss as the loss function, further improving the model's robustness to imbalanced data. Additionally, to strengthen the nonlinear representation capacity of the architecture, we implement CELU activation functions as substitutes for conventional ReLU units within residual modules. According to experimental results, we can find that the proposed model achieves a classification accuracy of 97% in a -6dB noise environment, which is an improvement of 1.07 percentage points over the original MobileNetV3 model, demonstrating the effectiveness of this approach in fan anomaly detection.},
  address = {New York, NY, USA},
  author = {Sun, Fuyue},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {6},
  pages = {453–458},
  publisher = {Association for Computing Machinery},
  title = {The Fan Anomaly Detection Method Based on an Improved CBAM Attention Mechanism with MobileNetV3},
  url = {https://doi.org/10.1145/3728725.3728796},
  year = {2025}
}

@inbook{merged776,
  abstract = {Aiming at the traditional fall detection algorithms that suffer from low detection accuracy and poor real-time performance in dealing with small targets and multi-scale features in complex scenes, this study proposes a fall detection algorithm based on an improved YOLOv8 model. First, the SCConv module is utilized to combine with the C2f module in the YOLOv8 backbone network to construct the C2fSC module, which enhances the feature extraction capability of the model while reducing the computational complexity. Second, the SPPF module is integrated with the BAM attention mechanism to weaken the influence of background interference. Finally, WIoU is used to replace the original loss function to optimize the positioning accuracy of the bounding box and enhance the overall performance of the model. The experimental results show that compared with the original YOLOv8 model, the improved model improves 4.9%, 4.2%, and 4.4% in P, R, and mAP metrics, respectively, and the number of parameters and FLOPs are only 2.8M and 7.7G, which proves that the improved model has excellent robustness and effectiveness.},
  address = {New York, NY, USA},
  author = {Cai, Litao and Liu, Mingmei and Feng, Youbing and Xiong, Feng},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {6},
  pages = {610–615},
  publisher = {Association for Computing Machinery},
  title = {Research on fall detection algorithm based on improved YOLOv8},
  url = {https://doi.org/10.1145/3728725.3728821},
  year = {2025}
}

@inbook{merged777,
  abstract = {Through the lens of the Temporal-Synergic Cross-Correlation (TSCC) framework, this study fills important gaps in cross-border demand forecasting. Overall, analyzing 9,754 transactions (2022–2024) our approach captures multi-scale temporal decomposition synergized with SHAP-based feature engineering and outperforms the LSTM-RF hybrids by 22% precision and discovers 14.7% YoY revenue growth. Notably, the innovations tackled temporal-categorical disconnects and 92% holiday prediction precision — 18.4% on global metrics. The framework bridges theoretical models and operational logistics, harvesting trials and errors to overcome cross-border commerce lead-time preserves.},
  address = {New York, NY, USA},
  author = {Yao, Xiongfei},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {7},
  pages = {498–504},
  publisher = {Association for Computing Machinery},
  title = {Demand Forecasting for Cross-border Retail Commodities in the New Retail Era: A Hybrid Model Integrating Temporal Granularity and Categorical Dynamics},
  url = {https://doi.org/10.1145/3728725.3728803},
  year = {2025}
}

@inbook{merged778,
  abstract = {In the context of the popularity of Transformer model services, the issue of privacy protection has gradually gained attention. However, the primary issues with existing private inference solutions lie in memory constraints and suboptimal computational performance. In this work, we introduce PPQformer, a framework that leverages Replicated Secret Sharing (RSS) and quantization techniques to enable efficient and secure inference of Transformer models in a Multi-Party Computation (MPC) setting. By integrating quantization into the MPC protocols, PPQformer significantly reduces memory footprint and enhances computational performance while preserving data and model privacy. Experimental results demonstrate the effectiveness of our approach, achieving competitive accuracies with reduced communication cost and inference time compared to existing methods.},
  address = {New York, NY, USA},
  author = {Dong, Benchang and Chen, Zhili},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {5},
  pages = {115–119},
  publisher = {Association for Computing Machinery},
  title = {PPQformer: Privacy-Preserving Quantized Transformer for Efficient and Secure Inference.},
  url = {https://doi.org/10.1145/3728725.3728743},
  year = {2025}
}

@inbook{merged779,
  abstract = {The use of mobile phone APP meter reading reduces the probability of human intervention and misidentification, but due to the complex location and geographical conditions of the buried water meter, as well as the base code and gear in the water meter dial, there are problems of low accuracy and rough segmentation results when the water meter word wheel region is divided, so a YOLO v8 segmentation method based on coordinate attention mechanism is proposed. The backbone network incorporates the AM (Attention-MobileNet_block) module to apply the Coordinate Attention (CA) mechanism, which facilitates the extraction of multi-scale features, reduces model parameters and computational complexity, and enhances computing efficiency. In the feature fusion stage, the C2f_CA mechanism, based on the Bottleneck_CA module, effectively mitigates background interference while concentrating on the water meter's target region. Experimental results indicate that, compared to other segmentation models, our model achieves an F1-score of 98.61% and a mean Pixel Accuracy (mPA) of 99.11% for word wheel region segmentation, establishing a solid foundation for the precise identification of water meter code words in the future.},
  address = {New York, NY, USA},
  author = {Wang, Hongjuan and Wang, Nan and An, Xinjun},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {6},
  pages = {278–283},
  publisher = {Association for Computing Machinery},
  title = {CAM-YOLO v8: A model for water meter word wheel region segmentation},
  url = {https://doi.org/10.1145/3728725.3728769},
  year = {2025}
}

@inproceedings{merged780,
  abstract = {As deep learning technology in computer science develops, generative artificial intelligence (GenAI) has shown great potential in automated essay scoring (AES). Prompts and time are important factors which may influence the performance of GenAI. The study selected 52 English argumentative essays, designed five different prompts, chooses two different time points, and then utilized ChatGPT 4 to explore the effects of prompts and time on AES by GenAI. Besides, possible reasons for large difference between human and GenAI score were discussed.For different prompts, results show that the one-shot prompt performs better in AES compared with the other four prompts. It provides background information and a scoring example to ChatGPT. The scores generated by it do not significantly differ from the human scores. They significantly and positively correlate with human scores (ρ = 0.424). The exact-plus-adjacent agreement (EPAA) rate for one-shot prompt scores reaches 69.23%. For scores generated at different points in time, results reveal that although there is still a significant difference between scores generated after one week and human scores, the ChatGPT-Human EPAA rate becomes higher and the absolute value of mean score difference is smaller.Based on the analysis of selected essays, the major reason for large GenAI-Human score difference is that ChatGPT evaluates essays from limited perspectives to give its score, while human raters can comprehensively assess the quality of an essay. What's more, ChatGPT cannot keep the same scoring criteria during the rating process.The study aims to help people understand how to interact with GenAI more efficiently and take advantage of GenAI to meet the practical needs.},
  address = {New York, NY, USA},
  author = {Lin, Daping and Pu, Xianwei},
  booktitle = {Proceedings of the 2024 International Symposium on Artificial Intelligence for Education},
  doi = {10.1145/3700297.3700350},
  isbn = {9798400707100},
  keywords = {English argumentative essays; automated essay scoring; generative artificial intelligence; prompts; time},
  numpages = {11},
  pages = {302–312},
  publisher = {Association for Computing Machinery},
  series = {ISAIE '24},
  title = {Effects of Prompts and Time on the Automated Scoring of English Argumentative Essays by ChatGPT 4},
  url = {https://doi.org/10.1145/3700297.3700350},
  year = {2024}
}

@inproceedings{merged781,
  abstract = {The electronic design automation (EDA) community has recently begun recognizing the potential of generative artificial intelligence (AI) in chip design. However, its full potential is not fully exploited due to the limited availability of publicly accessible datasets crucial for advancing research in EDA. This paper highlights the dual role of generative AI; in particular, it showcases (i) BeGAN, the use of a generative AI strategy to create thousands of realistic benchmarks for power grid synthesis and analysis to advance power-related research, and (ii) EDA Corpus---an expert-curated and generative AI-enhanced dataset to serve research and development of EDA tool assistants. These two case studies emphasize the ability of generative methods to create and utilize datasets to advance research and lower the barriers to entry in EDA.},
  address = {New York, NY, USA},
  articleno = {57},
