@inproceedings{merged113,
  abstract = {The integration of Generative Artificial Intelligence (GAI) in education has garnered increasing attention due to its transformative potential in classroom teaching. Using grounded theory, this study analyzes relevant policy texts and teaching case studies to construct a comprehensive framework for GAI applications in educational settings. Findings indicate that GAI can enhance personalized learning, foster improved teacher-student interactions, and streamline educational processes, providing novel pathways for educational innovation.},
