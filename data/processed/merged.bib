  publisher = {Association for Computing Machinery},
  series = {CHI EA '25},
  title = {Generative AI in Special Education: Teachers' Insights on Instructional Enrichment vs. Accommodations},
  url = {https://doi.org/10.1145/3706599.3719972},
  year = {2025}
}

