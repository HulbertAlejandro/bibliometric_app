  pages = {1489–1493},
  publisher = {Association for Computing Machinery},
  title = {Integrating Artificial Intelligence into Vocational Education: A Technical Application Framework Based on the 3E Model},
  url = {https://doi.org/10.1145/3745238.3745471},
  year = {2025}
}

@inproceedings{merged1326,
  abstract = {This research work detailed the utilization of Artificial Intelligence (AI) particularly in the development of four-grid canvas paintings. This is possible by integrating the computational power of AI with conventional art designs. AI-based approaches play a significant role in art creation which includes human emotions and exactness. Four-grid painting is an artistic approach that splits the canvas into four separate distinctive sections. Each section of the canvas illustrates a diverse topic. This work comes across the history of four-grid canvas paintings, state-of-the-art algorithms, and architectures of AI drawing methods. We discuss one of the prominent AI platforms called Midjourney and its use in creating four-grid paintings. Furthermore, this paper focuses on the employment of AI in building four-grid paintings and the product design. This illustrates the advancements in the field of amusement, healing through art, and visual aesthetics. Numerous factors have been the focus of a comparative analysis of various AI platforms. These findings will assist in the development of AI-enhanced art.},
