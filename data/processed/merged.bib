  series = {ICCAD '24},
  title = {Generative Methods in EDA: Innovations in Dataset Generation and EDA Tool Assistants},
  url = {https://doi.org/10.1145/3676536.3697122},
  year = {2025}
}

@article{merged782,
  abstract = {The rapid development of generative artificial intelligence has brought significant opportunities for the advancement of digital humanities research. Intelligent processing of ancient texts, as an essential part of digital humanities, is also undergoing a transformation in research methodologies in the wave of AIGC. The integration of generative pre-trained models with Chinese ancient texts, a vital carrier of Chinese culture, allows for deep mining of the content of these texts and provides services that make ancient texts more understandable and accessible to the general public. In this research, we propose a method that combines the most renowned Chinese anthology, the “Siku Quanshu,” with generative pre-trained models. We developed the SikuGPT model, a generative model for ancient text processing tasks, based on GPT-type language models by continued pretraining. This model was tested on two typical tasks of ancient text processing: translation between classical and modern Chinese, and classification of ancient texts. The findings reveal that our model achieves advantages in understanding and generating scenarios of ancient texts. The capability of SikuGPT in processing traditional Chinese texts helps to promote the organization of ancient information and knowledge services, and advances the international dissemination of traditional Chinese culture.},
  address = {New York, NY, USA},
  articleno = {53},
  author = {Liu, Chang and Wang, Dongbo and Zhao, Zhixiao and Hu, Die and Wu, Mengcheng and Lin, Litao and Liu, Jiangfeng and Zhang, Hai and Shen, Si and Li, Bin and Zhao, Lianzhen},
  doi = {10.1145/3676969},
  issn = {1556-4673},
  issue_date = {December 2024},
  journal = {J. Comput. Cult. Herit.},
  keywords = {Chinese ancient texts; Generative pre-trained model; Siku Quanshu; digital humanities research; natural language processing},
  month = {December},
  number = {4},
  numpages = {17},
  publisher = {Association for Computing Machinery},
  title = {SikuGPT: A Generative Pre-trained Model for Intelligent Information Processing of Ancient Texts from the Perspective of Digital Humanities},
  url = {https://doi.org/10.1145/3676969},
  volume = {17},
  year = {2024}
}

@inproceedings{merged783,
  abstract = {Content creators increasingly utilize generative artificial intelligence (Gen-AI) on platforms such as YouTube, TikTok, Instagram, and various blogging sites to produce imaginative images, AI-generated videos, and articles using Large Language Models (LLMs). Despite its growing popularity, there remains an underexplored area concerning the specific domains where AI-generated content is being applied, and the methodologies content creators employ with Gen-AI tools during the creation process. This study initially explores this emerging area through a qualitative analysis of 68 YouTube videos demonstrating Gen-AI usage. Our research focuses on identifying the content domains, the variety of tools used, the activities performed, and the nature of the final products generated by Gen-AI in the context of user-generated content.},
  address = {New York, NY, USA},
