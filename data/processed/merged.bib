@inproceedings{merged408,
  abstract = {Chinese martial arts are deeply rooted in the traditional culture of the Chinese nation. Throughout its historical development, it has accumulated a profound historical heritage and formed its unique cultural value. The technological innovation of generative artificial intelligence provides a new path for the modernization transformation of Chinese martial arts culture. This study proposes a framework for Chinese martial arts cultural products based on generative artificial intelligence and multimodal interaction. By combining deep learning models (such as GANs, Transformers) with blockchain and AR/VR technology, five applications were designed: technology-enabled martial arts digital products, martial arts interactive short videos, martial arts motion capture systems, martial arts education platforms, and martial arts virtual experience halls. These applications were evaluated and validated from the dimensions of user experience and market potential. Experimental results show that martial arts cultural products under generative artificial intelligence and multimodal interaction have good user experience and positive market prospects, providing theoretical and practical reference for the deep integration of martial arts culture and intelligent technology.},
  address = {New York, NY, USA},
  author = {Cheng, Guo and Liu, Peng},
  booktitle = {Proceedings of the 2nd Guangdong-Hong Kong-Macao Greater Bay Area Education Digitalization and Computer Science International Conference},
  doi = {10.1145/3746469.3746516},
  isbn = {9798400713811},
  keywords = {Cross-modal Interaction; Deep Learning; Generative AI; Intelligent System Design; Martial Arts Cultural Products},
