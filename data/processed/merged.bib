  month = {February},
  number = {1},
  numpages = {10},
  publisher = {Association for Computing Machinery},
  title = {Artificial Intelligence and the Future of Evaluation: From Augmented to Automated Evaluation},
  url = {https://doi.org/10.1145/3696009},
  volume = {6},
  year = {2025}
}

@article{merged1257,
  abstract = {Artificial Intelligence (AI) is making a profound impact in almost every domain. A vital enabler of its great success is the availability of abundant and high-quality data for building machine learning models. Recently, the role of data in AI has been significantly magnified, giving rise to the emerging concept of data-centric AI. The attention of researchers and practitioners has gradually shifted from advancing model design to enhancing the quality and quantity of the data. In this survey, we discuss the necessity of data-centric AI, followed by a holistic view of three general data-centric goals (training data development, inference data development, and data maintenance) and the representative methods. We also organize the existing literature from automation and collaboration perspectives, discuss the challenges, and tabulate the benchmarks for various tasks. We believe this is the first comprehensive survey that provides a global view of a spectrum of tasks across various stages of the data lifecycle. We hope it can help the readers efficiently grasp a broad picture of this field, and equip them with the techniques and further research ideas to systematically engineer data for building AI systems. A companion list of data-centric AI resources will be regularly updated on .},
  address = {New York, NY, USA},
  articleno = {129},
  author = {Zha, Daochen and Bhat, Zaid Pervaiz and Lai, Kwei-Herng and Yang, Fan and Jiang, Zhimeng and Zhong, Shaochen and Hu, Xia},
  doi = {10.1145/3711118},
  issn = {0360-0300},
  issue_date = {May 2025},
  journal = {ACM Comput. Surv.},
  keywords = {Artificial intelligence; data-centric AI; machine learning},
  month = {January},
  number = {5},
  numpages = {42},
  publisher = {Association for Computing Machinery},
  title = {Data-centric Artificial Intelligence: A Survey},
  url = {https://doi.org/10.1145/3711118},
  volume = {57},
  year = {2025}
}

@inproceedings{merged1258,
  abstract = {The design of English teaching system based on artificial intelligence technology aims to solve the problems of insufficient training time and low efficiency in traditional English teaching. The research adopts advanced technologies such as intelligent speech recognition, natural language processing and machine learning to build an efficient and interactive learning environment. By integrating virtual reality and augmented reality technology, the system strengthens students' language application ability and encourages communication in simulated or actual contexts. The classroom teaching mode covers three stages: before class, during class and after class, to ensure the comprehensive improvement of students' listening and speaking skills. ITS module design includes student model, teacher model and expert model, which realizes the adjustment of personalized teaching content, evaluates learning results and provides targeted solutions. The realization of system functions depends on three core components: remote courseware service system, customer mobile learning system and data agent system, providing a stable and efficient personalized adaptive learning experience. The test results show that the system can still maintain high efficiency and stability when the number of concurrent users increases, which is superior to the traditional teaching system. In addition, intelligent composition correction, oral practice feedback, personalized learning plan recommendation and intelligent translation tools have significantly improved students' English writing ability and oral fluency.},
  address = {New York, NY, USA},
  author = {Meng, Yanrong},
  booktitle = {Proceeding of the 2024 International Conference on Artificial Intelligence and Future Education},
  doi = {10.1145/3708394.3708414},
  isbn = {9798400710650},
  keywords = {English; artificial intelligence technology; teaching system},
  numpages = {5},
  pages = {109–113},
  publisher = {Association for Computing Machinery},
  series = {AIFE '24},
  title = {Design of English Teaching System Based on Artificial Intelligence Technology},
  url = {https://doi.org/10.1145/3708394.3708414},
  year = {2025}
}

@inbook{merged1259,
  abstract = {This paper systematically discuss the English standardization in artificial intelligence (AI) and smat manunfacturing, focuses on related problems, means and practises. Aimed at the AI and smart manufacturing's development in large volume and speed, English standard terminology could make international cooperation, information communication and industry standard more effective. There are main difficulties include the multidenoting and the obscurity of words, the rate new jargon, the confusion difference of industry and in language-culture difficulties, particularly language not English.For these challenges, the paper suggests ways: to establish authoritative terminology databases (i.e., ISO/TC 37) that regularly updates their definitions and use for latest version; to intensify collaborations of global and national standard bodies to create one single norm, develop one single standard to define their terminologies usage, to design for the rules of translation to reflect consistent terminologies use, and professionals’ terminology literacy in using terms. Three cases from authors show how those approaches help cross-industry and cross-cultural communications, including use of digital twin to advance the development of aircrafts, automotive, and automobile production in detail.Our research attempt combining linguistics, artificial intelligence, and manufacturing engineering in the presentation of not only theoretical foundations but also practical references for assisting effective international cooperation and normalized development of AI and smart manufacturing. In the further study, we expand the case of study on larger ranges of cultures and industrial fields in order to realize more effective terminology standardization for the current emerging technologies.},
  address = {New York, NY, USA},
  author = {Zhang, Yiyang},
