  publisher = {Association for Computing Machinery},
  series = {ISAIMS '23},
  title = {Psychological Healing in the Digital Age: A Study of Personalized Collaborative Models Empowered by GAI},
  url = {https://doi.org/10.1145/3644116.3644221},
  year = {2024}
