  publisher = {Association for Computing Machinery},
  series = {GAIIS '24},
  title = {Analysis of Traffic Flow Prediction Based on Historical Highway Traffic Volume},
  url = {https://doi.org/10.1145/3665348.3665416},
  year = {2024}
}

@inproceedings{merged644,
  abstract = {Unmanned aerial vehicles (UAV) are susceptible to unsecure communication problems in denied environments because of strong interferences from physical fields and electromagnetic fields. A dynamic key management scheme based on Chinese remainder theorem (CRT) and the Hash function is proposed here to solve this problem. Specifically, as changes in the topological structure and unstable communication are likely to cause security problems in communication of drones in the battlefield, two dynamic key update protocols, i.e., topology-aware key update and mutual-healing key update, are put forth here to ensure secure communication through dynamically-updated keys. Analyses reveal that the proposed scheme has forward and backward security and resistance to collusion attacks and replay attacks. Meanwhile, the proposed method has been compared with existing methods through tests on the popular MIRACL dataset by Visual Studio 2019, and the test results show that our method optimizes the computing performance and the communication overhead, and achieves higher updating efficiency.},
  address = {New York, NY, USA},
  author = {Yuan, Lin and Feng, Zhishang},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3665348.3665400},
  isbn = {9798400709562},
  location = {Kuala Lumpur, Malaysia},
  numpages = {10},
  pages = {303–312},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '24},
  title = {A dynamic key update scheme for UAV clusters in denied environments},
  url = {https://doi.org/10.1145/3665348.3665400},
  year = {2024}
}

@inproceedings{merged645,
  abstract = {With the rapid expansion of the mobile commerce industry, the demand for visual design has increased sharply, and the shortage of professional designer has become a bottleneck restricting the development of the industry. In order to solve this contradiction, this paper focuses on the application research of AI painting technology, especially through a series of computer optimization techniques to improve key links such as image preprocessing, model selection and output optimization, so as to better meet the unique needs of visual communication in mobile commerce. In this paper, the image preprocessing is divided into portrait, product and scene, which are classified according to the characteristics of the collected images, and then different LoRA models are trained to match the Checkpoint model to generate images that meet the characteristics of mobile commerce, thus improving the application efficiency and flexibility of AI painting. Through the selection and optimization of these technologies, AI painting technology can more effectively adapted to the production mode of mobile business design, thus improving the work efficiency of the industry, reducing the dependence on human resources, and providing new solutions for the development of the industry..},
  address = {New York, NY, USA},
  author = {Lu, Yao and He, Sixuan and Zhong, Yuhan},
  booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
  doi = {10.1145/3734921.3734924},
  isbn = {9798400713460},
  keywords = {Design features of mobile commerce; Efficiency improvement; Image preprocessing},
  numpages = {5},
  pages = {14–18},
  publisher = {Association for Computing Machinery},
  series = {GADM '25},
  title = {Research on the Application of AI Painting Technology in Mobile Commerce Design},
  url = {https://doi.org/10.1145/3734921.3734924},
  year = {2025}
}

@inbook{merged646,
  abstract = {Shipboard emergencies can easily induce secondary events because of any time and space transition. Carrier-based aircraft emergencies have complexity, tight coupling and strong real-time characteristics. Therefore, it is of great significance to reasonably predict the development trend of the incident in the emergency treatment to reduce the harm. Due to the sudden and random nature of naval battlefield accidents, some incidents are not covered by the historical case base. Traditional retrieval approaches to event prediction using case knowledge are limited by the sparsity of events and the cold-start problem. To address these challenges, this paper proposes a multi-graph neural network for event development trend prediction, which disassembles events into event features and introduces the shipboard aircraft domain knowledge graph. We produce a data set around the event features of carrier-based aircraft on the simulation platform. The results show that the graph neural network with knowledge graph is superior in modeling the higher-order semantic association between event features. Compared to state-of-the-art baseline methods, our approach exhibits significant improvements.},
  address = {New York, NY, USA},
  author = {Wang, Yawen and Jin, Yuanyuan},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {6},
  pages = {20–25},
  publisher = {Association for Computing Machinery},
  title = {Knowledge-Enhanced Event Prediction of Carrier-Based Aircraft Emergencies},
  url = {https://doi.org/10.1145/3728725.3728729},
  year = {2025}
}

@inproceedings{merged647,
  abstract = {In scenarios with fixed time intervals, discrete-time dynamic graph (DTDG) is used to represent dynamic networks, such as social networks, traffic networks and biological networks. Existing models introduce self-attention mechanism for structural representation learning on discrete-time dynamic graph and demonstrate good performance in this field. However, temporal encoder in these models focuses on the entire time domain, introducing noise and limiting the model's ability to extract critical information. To address this issue, we propose the DyLF model based on Longformer mechanism. By incorporating the Longformer-based self-attention mechanism, DyLF can flexibly adjust the range of visible time domain while encoding temporal features of nodes in dynamic graphs and efficiently capture temporal dependencies between snapshots, thereby enhancing the ability to extract more effective temporal information. Extensive experiments show that DyLF achieves an average improvement of 4.28% in AUC and 6.87% in F1_score with the existing baseline, demonstrate that it is better suited for handling DTDGs.},
  address = {New York, NY, USA},
  author = {Zhang, Anxin},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3728725.3728771},
  isbn = {9798400713453},
  keywords = {Discrete-time Dynamic Graph; Graph Representation Learning; Longformer; Self-attention},
  numpages = {6},
  pages = {289–294},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '25},
  title = {DyLF: Introducing Longformer for More Efficient Dynamic Graph Learning},
  url = {https://doi.org/10.1145/3728725.3728771},
  year = {2025}
}

@inproceedings{merged648,
  abstract = {The purpose of this study is to explore the application of generative artificial intelligence (AI) in Chinese literacy teaching in the first grade of primary school and its influence on teachers' practical knowledge construction and teaching innovation. Through two rounds of action research, this study deeply analyzes the practical effect of AI tools in assisting teachers in learning situation analysis, personalized teaching design and classroom interaction. Through the analysis of process data and interviews such as AI use process record form and classroom observation scale, the research found that, on the one hand, the learning situation analysis function of AI tools provides strong data support for teachers through in-depth insight into students' literacy ability and learning style. On the other hand, the iterative dialogue between AI and teachers promotes teachers 'in-depth understanding of literacy teaching content, and stimulates their innovative teaching thinking. The proper application of generative AI technology in the field of education not only improves teaching efficiency, but also provides a new way for teachers' professional development and promotion of practical knowledge.},
  address = {New York, NY, USA},
  author = {Huang, Cuitian and Yu, Hong},
  booktitle = {Proceedings of the 2024 International Conference on Intelligent Education and Computer Technology},
  doi = {10.1145/3687311.3687419},
  isbn = {9798400709920},
  location = {Guilin, China},
  numpages = {6},
  pages = {606–611},
  publisher = {Association for Computing Machinery},
  series = {IECT '24},
  title = {Technological Dialogue and Intelligent Generation: An Empirical Study of Primary Chinese Teachers Using Generative AI for Lesson Preparation},
  url = {https://doi.org/10.1145/3687311.3687419},
  year = {2024}
}

@inproceedings{merged649,
  abstract = {This Special Interest Group (SIG) explores the transformative impact of Generative Artificial Intelligence (GenAI) on Human-Computer Interaction (HCI) research processes. The theme here is to answer “question zero”: when to use and when to refrain from using AI tools during the research cycle? The discussion is guided by five research phases commonly used in HCI: research planning, prototyping, data collection, analysis and synthesis, and dissemination and communication. We investigate how GenAI accelerates project cycles, enhances reproducibility, and influences inclusivity in research. We also address the challenging ethical considerations about the ownership of generated content. Our goal is to build a community of HCI enthusiasts to harness the early advantages of the recent groundbreaking technology and foresee challenges arising from its prevalence in the scientific community.},
  address = {New York, NY, USA},
  articleno = {584},
  author = {Elagroudy, Passant and Li, Jie and V\"{a}\"{a}n\"{a}nen, Kaisa and Lukowicz, Paul and Ishii, Hiroshi and Mackay, Wendy E. and Churchill, Elizabeth F and Peters, Anicia and Oulasvirta, Antti and Prada, Rui and Diening, Alexandra and Barbareschi, Giulia and Gruenerbl, Agnes and Kawaguchi, Midori and El Ali, Abdallah and Draxler, Fiona and Welsch, Robin and Schmidt, Albrecht},
  booktitle = {Extended Abstracts of the CHI Conference on Human Factors in Computing Systems},
  doi = {10.1145/3613905.3643977},
  isbn = {9798400703317},
  keywords = {ChatGPT; Generative AI; HCI research; Large Language Models; Large Multimodal Models; research processes; science},
  location = {Honolulu, HI, USA},
  numpages = {5},
  publisher = {Association for Computing Machinery},
  series = {CHI EA '24},
  title = {Transforming HCI Research Cycles using Generative AI and “Large Whatever Models” (LWMs)},
  url = {https://doi.org/10.1145/3613905.3643977},
  year = {2024}
}

@inbook{merged650,
  abstract = {The Internet has enabled online learning to become an important means for students to acquire knowledge, and student cognitive diagnosis models have played a key role in this process. These models can evaluate students' learning behaviors and knowledge mastery, providing support for personalized learning. Unfortunately, traditional models have their limitations when dealing with complex relational data related mainly to the relationship between students and problems, while the characteristics as well as potential relationships among students are almost left neglected. In response, this paper proposes a Neural Network Student Cognitive Diagnosis Model Based on Multi-Relational Graph (MGNCD) that brings together the powers of Graph Attention Network (GAT) and Relational Graph Convolutional Network (RGCN) to model multi-dimensional heterogeneous nodes such as students, classes, problems, and skills. The introduction of various edge types and random edge connection strategies allowed the model to tackle data sparsity and heterogeneity. Experimental results indicate that the proposed model outperforms traditional methods in accuracy (ACC) and area under the curve (AUC), which provides an encouraging novel insight into cognitive diagnosis.},
  address = {New York, NY, USA},
  author = {Han, Ze and Su, Yu and Mo, Qi and Nie, Xinyu},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {7},
  pages = {515–521},
  publisher = {Association for Computing Machinery},
  title = {Neural Network Student Cognitive Diagnosis Model Based on Multi-Relational Graph},
  url = {https://doi.org/10.1145/3728725.3728806},
  year = {2025}
}

@inbook{merged651,
  abstract = {This paper presents an Android certificate validation vulnerability detection framework that integrates static analysis, adversarial sample training, and optimization via large language models. Centered around the Wasserstein Generative Adversarial Network, the framework aims to enhance the robustness and generalization ability of static detection models against complex and obfuscated attack patterns. Static analysis is performed on 2,000 popular Android applications from the androzoo dataset, with code and configuration features extracted, focusing on certificate-related API usage, permission declarations, and network security settings. Among them, 110 APKs are identified as containing high-risk certificate misconfigurations. Based on these features, a Transformer-based model is constructed for feature vectorization and vulnerability identification. To further improve model robustness, WGAN is employed to generate adversarial samples that mimic real-world evasive vulnerabilities, which are used for adversarial training. Additionally, large language models are used to perform In-Context Learning, enabling semantic filtering, sample refinement, and iterative generator feedback. This results in a closed-loop, self-improving detection pipeline that combines detection, optimization, and feedback. Experimental results across standard, adversarial, and cross-domain scenarios demonstrate that the proposed framework achieves an F1 score of 0.91 and reduces the false positive rate to 7.2%, significantly outperforming traditional static detection tools in both accuracy and resilience.},
  address = {New York, NY, USA},
  author = {Fang, Yong},
  booktitle = {Proceedings of the 2025 2nd International Conference on Generative Artificial Intelligence and Information Security},
  isbn = {9798400713453},
  numpages = {6},
  pages = {231–236},
  publisher = {Association for Computing Machinery},
  title = {Wasserstein GAN-Based Android Certificate Validation Vulnerability Detection Framework},
  url = {https://doi.org/10.1145/3728725.3728761},
  year = {2025}
}

@inproceedings{merged652,
  abstract = {With the rapid development of 5G and mobile internet, various application fields continue to generate diverse dynamic streaming graphs with large scales and complex structures. Analyzing, querying and mining such a streaming graph face great challenges. To address the challenge of low query efficiency and avoid data loss, this paper proposes a two-stage coarsening method that preserves key features of a streaming graph while improves query efficiency. In the initial stage, the size of the streaming graph is not that large yet. To preserve its structure and attributes as much as possible, a linear mapping method is used for graph coarsening. As the size of the streaming graph increases, a graph neural network model based on cluster pooling is constructed in the second stage. Using a large number of graph coarsening results obtained in the initial stage to train and optimize this model. Then, we use this optimized model to obtain the subsequent summaries, which achieves online efficient graph coarsening. Finally, extensive experiments indicate that our algorithm significantly improves the quality of streaming graph coarsening compared to traditional algorithms. Even compared to the state-of-the-art algorithm Auxo, our algorithm shows a better performance on preserving key features.},
  address = {New York, NY, USA},
  author = {Sun, Zhipeng and Zeng, Guosun},
  booktitle = {Proceedings of the 2024 International Conference on Generative Artificial Intelligence and Information Security},
  doi = {10.1145/3665348.3665392},
  isbn = {9798400709562},
  location = {Kuala Lumpur, Malaysia},
  numpages = {8},
  pages = {253–260},
  publisher = {Association for Computing Machinery},
  series = {GAIIS '24},
  title = {A Two-stage Coarsening Method for a Streaming Graph with Preserving Key Features},
  url = {https://doi.org/10.1145/3665348.3665392},
  year = {2024}
}

@inproceedings{merged653,
  abstract = {With the development of digital image technology, single image super-resolution (SISR) has made unprecedented progress. However, the existing SISR technology based on convolutional neural network (CNN) still faces great challenges in the application of resource-constrained mobile devices. For lightweight SISR, we propose a multi-level feature mixing network (MFMN), which achieves high-quality super-resolution image reconstruction at low computational cost. Based on re-parameterization, a multi-level feature generation module (MFGM) is designed to extract feature mappings from different perspectives. Then, to enhance feature details, a feature refinement module (FRM) is constructed to aggregate high-frequency information, which can assign weight values to pixels in feature maps according to the importance of different hierarchical features. Through many experiments, our proposed method shows excellent image reconstruction results in a lightweight way on multiple benchmark datasets and exceeds the current advanced methods.},
  address = {New York, NY, USA},
  author = {Xu, Feifei and Chen, Lulu},
  booktitle = {Proceedings of the 2025 International Conference on Generative Artificial Intelligence and Digital Media},
  doi = {10.1145/3734921.3734936},
  isbn = {9798400713460},
  keywords = {convolutional neural network (CNN); feature refinement; multi-level feature; single image super-resolution (SISR)},
  numpages = {6},
  pages = {89–94},
  publisher = {Association for Computing Machinery},
  series = {GADM '25},
  title = {A Lightweight Multi-level feature Mixing network For Image Super-Resolution},
  url = {https://doi.org/10.1145/3734921.3734936},
  year = {2025}
}

@article{merged654,
  abstract = {The rapid integration of generative artificial intelligence (GenAI) across diverse fields underscores the critical need for red teaming efforts to proactively identify and mitigate associated risks. While previous research primarily addresses technical aspects, this paper highlights organizational factors that hinder the effectiveness of red teaming in real-world settings. Through qualitative analysis of 17 semi-structured interviews with red teamers from various organizations, we uncover challenges such as the marginalization of vulnerable red teamers, the invisibility of nuanced AI risks to vulnerable users until post-deployment, and the lack of user-centered red teaming approaches. These issues often arise from underlying organizational dynamics, including organizational resistance, organizational inertia, and organizational mediocracy. To mitigate these dynamics, we discuss the implications of user research for red teaming and the importance of embedding red teaming throughout the entire development cycle of GenAI systems.},
  address = {New York, NY, USA},
  articleno = {CSCW460},
  author = {Ren, Bixuan and Cheon, EunJeong and Li, Jianghui},
  doi = {10.1145/3757641},
  issue_date = {November 2025},
  journal = {Proc. ACM Hum.-Comput. Interact.},
  keywords = {AI harm; AI safety; adversarial test; red teaming; work practice},
  month = {October},
  number = {7},
  numpages = {26},
  publisher = {Association for Computing Machinery},
  title = {Organization Matters: A Qualitative Study of Organizational Dynamics in Red Teaming Practices For Generative AI},
  url = {https://doi.org/10.1145/3757641},
  volume = {9},
  year = {2025}
}

@inproceedings{merged655,
  abstract = {Generative artificial intelligence (generative AI) has seen a meteoric rise in capability, popularity, and use. As the popularity of this technology grows, protocols are being developed around how to manage the use of generative AI in academic spaces. It is imperative that the games and play research community join this important conversation, and discuss the appropriateness of using generative AI in both digital games, and our own academic works.},
