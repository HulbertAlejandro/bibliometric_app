@inproceedings{merged1281,
  abstract = {Scientific creativity (i.e., to innovate new scientific approaches or ideas) is a fundamental ability for scientists. Since the rise of artificial intelligence (AI), researchers have used specialized models to improve or automatize scientific processes and even scientific discoveries. In our study, we investigate whether proprietary large language models such as ChatGPT, though not specifically trained for scientific work, are able to convincingly perform or even surpass human scientific creativity. We asked graduate students and prompted ChatGPT to write an abbreviated version of a research proposal for a fictious scenario from the field of computer science/ robotics. Four specific assignments were given (i.e., formulating a hypothesis, designing an experiment, listing the required equipment, and justifying the chosen method). Human and AI-generated answers were rated by two scientists from the field, following a peer-review approach. Our results indicate that ChatGPT reached significantly higher overall scores relative to the human sample. However, closer inspection of the data and the rating procedure revealed that the evaluation of performance especially in regards to the originality of ideas differed considerably between the two raters. We evaluate these findings in more detail, as well as qualitative differences between human and AI- generated responses. Lastly, we discuss the impact of raters’ knowledge about and attitudes towards Generative AI and emphasize the importance of meta-cognitive skills to identify Generative AI.},
  address = {New York, NY, USA},
  articleno = {41},
  author = {Eymann, Vera and Lachmann, Thomas and Hekele, Felix and Ashok, Ashita and Berns, Karsten and Czernochowski, Daniela},
  booktitle = {Proceedings of the 36th Annual Conference of the European Association of Cognitive Ergonomics},
  doi = {10.1145/3746175.3746208},
  isbn = {9798400720338},
  keywords = {Artificial Intelligence; Creativity; Generative AI; Scientific Creativity},
  numpages = {5},
  publisher = {Association for Computing Machinery},
  series = {ECCE '25},
  title = {Does ChatGPT know what robots are thinking? - Evaluation of human-created vs. AI-generated scientific ideas},
  url = {https://doi.org/10.1145/3746175.3746208},
  year = {2025}
}

