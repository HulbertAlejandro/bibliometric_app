  keywords = {Computational modeling; Education; Interviews; Physics; Portfolios; Problem-solving; computational thinking; physics classroom; project-based learning},
  month = {Oct},
  pages = {76-80},
  title = {Bringing Computational Thinking Skills Into Physics Classroom Through Project-Based Learning},
  year = {2022}
}

