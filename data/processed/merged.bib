}

@inproceedings{merged1249,
  abstract = {This study contributes to the global conversation on artificial intelligence (AI) literacy, with a specific focus on Uganda, a sub-Saharan African region that has received limited attention in AI education research. As AI becomes increasingly pervasive, it is essential to introduce AI concepts to young children to promote their AI literacy. However, the current landscape of AI education in Uganda is not well understood. To address this gap, this research explores a community-based approach to teaching AI and machine learning (ML) to young children in out-of-school settings. The study aims to discover the kinds of AI-related knowledge and skills that young children can develop and how they can learn about AI in a non-formal educational context. The study will employ a design science methodology (DSM) to create and evaluate practical solutions, including methods, models, and concepts, that can inform AI education in Uganda. By combining systematic literature reviews, qualitative and quantitative methods, the research will generate robust empirical evidence that supports the effectiveness and relevance of the proposed solutions. Through these methods, the research will explore the role of community-based approaches in fostering young children's AI literacy. The study will investigate the effectiveness of informal learning environments, including homes, museums, churches, and children's camps. This will provide design recommendations for community-based AI education programs and in doing so, the study addresses the limited initiatives on promoting AI education for everyone, particularly in developing regions using informal settings.},
  address = {New York, NY, USA},
  author = {Mabberi, Enock},
  booktitle = {Proceedings of the ACM Global on Computing Education Conference 2025 Vol 2},
  doi = {10.1145/3736251.3757101},
  isbn = {9798400719424},
  keywords = {artificial intelligence literacy; computational thinking; informal learning spaces; k-12 education; machine learning},
  location = {Gaborone, Botswana},
  numpages = {1},
  pages = {419},
  publisher = {Association for Computing Machinery},
  series = {CompEd 2025},
  title = {Developing Young Children Artificial Intelligence Literacy through Community-Based Approaches},
  url = {https://doi.org/10.1145/3736251.3757101},
  year = {2025}
}

@inproceedings{merged1250,
  abstract = {The EU Artificial Intelligence Act (AIA) establishes different legal principles for different types of AI systems. While prior work has sought to clarify some of these principles, little attention has been paid to robustness and cybersecurity. This paper aims to fill this gap. We identify legal challenges and shortcomings in provisions related to robustness and cybersecurity for high-risk AI systems (Art.&nbsp;15&nbsp;AIA) and general-purpose AI models (Art.&nbsp;55&nbsp;AIA). We show that robustness and cybersecurity demand resilience against performance disruptions. Furthermore, we assess potential challenges in implementing these provisions in light of recent advancements in the machine learning (ML) literature. Our analysis informs efforts to develop harmonized standards, guidelines by the European Commission, as well as benchmarks and measurement methodologies under Art.&nbsp;15(2)&nbsp;AIA. With this, we seek to bridge the gap between legal terminology and ML research, fostering a better alignment between research and implementation efforts.},
  address = {New York, NY, USA},
  author = {Nolte, Henrik and Rateike, Miriam and Finck, Mich\`{e}le},
  booktitle = {Proceedings of the 2025 ACM Conference on Fairness, Accountability, and Transparency},
  doi = {10.1145/3715275.3732020},
  isbn = {9798400714825},
