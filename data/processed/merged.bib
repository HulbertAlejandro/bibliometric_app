  issn = {1943-6106},
  keywords = {Computational Thinking; Concrete; Encoding; End-User Programming; Programming environments; Programming profession; Tangible User Interface; Visualization},
  month = {Sep.},
  pages = {232-233},
  title = {Fostering computational thinking skills with a tangible blocks programming environment},
